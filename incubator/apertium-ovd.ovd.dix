<pardef n="numeros">
  <!-- separate out the number 1 as singular (fortunately "1.00" is usually plural!) -->
  <e>       <re>1</re><p><l></l><r><s n="det"/><s n="qnt"/><s n="sg"/></r></p></e>
  <e>       <re>[02-9]</re><p><l></l><r><s n="det"/><s n="qnt"/><s n="pl"/></r></p></e>
  <e>       <re>[0-9][0-9]+([.,][0-9]+)?</re><p><l></l><r><s n="det"/><s n="qnt"/><s n="pl"/></r></p></e>
  <e>       <re>[0-9][.,][0-9]+</re><p><l></l><r><s n="det"/><s n="qnt"/><s n="pl"/></r></p></e>

  <e>       <re>[½¹²³¼¾]</re><p><l></l><r><s n="det"/><s n="qnt"/><s n="pl"/></r></p></e>
</pardef>

<pardef n="separa">
  <e>       <p><l>..</l><r>..<s n="sent"/><s n="clb"/></r></p></e>
  <e>       <p><l>...</l><r>...<s n="sent"/><s n="clb"/></r></p></e>
  <e>       <re>[.\?;:!…¿¶]</re><p><l></l><r><s n="sent"/><s n="clb"/></r></p></e>
</pardef>

<pardef n="coma">
  <e>       <re>,</re><p><l></l><r><s n="cm"/><s n="clb"/></r></p></e>
  <e>       <re>,</re><p><l></l><r><s n="cm"/></r></p></e>
</pardef>

<pardef n="aposchar">
