</pardef>

<pardef n="cometa">
  <e>       <par n="aposchar"/><p><l></l><r><s n="apos"/></r></p></e>
</pardef>

<pardef n="paresq">
  <e>       <re>[\(\[]</re><p><l></l><r><s n="lpar"/></r></p></e>
</pardef>

<pardef n="pardret">
  <e>       <re>[\)\]]</re><p><l></l><r><s n="rpar"/></r></p></e>
</pardef>


