	</rule>

	<rule comment="ADV">
      <pattern>
		<pattern-item n="adv"/>
      </pattern>
		<action>
			<out>
				<chunk name="advchunk">
					<tags>
              			<tag><lit-tag v="adv"/></tag>
            		</tags>
					<lu>
						<clip pos="1" side="tl" part="lemh"/>
						<lit-tag v="adv.sint"/>
					</lu>
				</chunk>
			</out>
		</action>
	</rule>

	<rule comment="Verb">
      <pattern>
		<pattern-item n="verb"/>
      </pattern>
		<action>
			<choose>
				<when>
					<test>
						<or>
				           <equal><clip pos="1" side="sl" part="temps"/><lit-tag v="ifi"/></equal>
				           <equal><clip pos="1" side="sl" part="temps"/><lit-tag v="pis"/></equal>
				           <equal><clip pos="1" side="sl" part="temps"/><lit-tag v="pii"/></equal>
				        </or>
			         </test>	
					<out>
						<chunk name="VerbCustom1">
						<tags>
		          			<tag><lit-tag v="verb_fti"/></tag>
		        		</tags>
							<lu>
								<lit v="biti"/>
								<lit-tag v="vbser.pres"/>
								<clip pos="1" side="tl" part="persona"/>
								<clip pos="1" side="tl" part="nbr"/>
							</lu>
							<b/>
							<lu>
								<clip pos="1" side="tl" part="lemh"/>
								<clip pos="1" side="tl" part="a_verb"/>
								<clip pos="1" side="tl" part="im_perf"/>
								<lit-tag v="past.m"/>
								<clip pos="1" side="tl" part="nbr"/>
							</lu>
						</chunk>
					</out>
				</when>
				<when>
					<test>
				        <equal><clip pos="1" side="sl" part="temps"/><lit-tag v="pp"/></equal>
			         </test>	
					<out>
						<chunk name="VerbPP">
						<tags>
		          			<tag><lit-tag v="verb_pp"/></tag>
		        		</tags>
							<lu>
								<clip pos="1" side="tl" part="lemh"/>
								<clip pos="1" side="tl" part="a_verb"/>
								<clip pos="1" side="tl" part="im_perf"/>
								<lit-tag v="past.m"/>
								<clip pos="1" side="tl" part="nbr"/>
							</lu>
						</chunk>
					</out>
				</when>
				<when>
					<test>
						<and>
				        <equal><clip pos="1" side="sl" part="temps"/><lit-tag v="cni"/></equal>
				        <equal><clip pos="1" side="sl" part="a_verb"/><lit-tag v="vbhaver"/></equal>
			         	</and>
			         </test>	
					<out>
						<chunk name="VerbPogojnik">
						<tags>
		          			<tag><lit-tag v="verb_cni"/></tag>
		        		</tags>
							<lu>
								<lit v="prpers"/>
								<lit-tag v="prn.pers"/>
								<clip pos="1" side="tl" part="persona"/>
								<lit-tag v="mfn"/> <!-- TO DO FIX: gender should be derivated from other words - chunk -->
								<clip pos="1" side="tl" part="nbr"/>
								<lit-tag v="nom"/>
							</lu>
							<b/>
							<lu>
								<lit v="biti"/>
								<lit-tag v="vbser.cni"/>
							</lu>
						</chunk>
					</out>
				</when>
				<when>
					<test>
						<and>
				        <equal><clip pos="1" side="sl" part="temps"/><lit-tag v="cni"/></equal>
				        	<or>
				        	<equal><clip pos="1" side="sl" part="a_verb"/><lit-tag v="vblex"/></equal>
				        	<equal><clip pos="1" side="sl" part="a_verb"/><lit-tag v="vbser"/></equal>
			         		</or>
			         	</and>
			         </test>	
					<out>
						<chunk name="VerbPogojnikVblex">
						<tags>
		          			<tag><lit-tag v="verb_cnivblex"/></tag>
		        		</tags>
							<lu>
								<clip pos="1" side="tl" part="lemh"/>
								<clip pos="1" side="tl" part="a_verb"/>
								<clip pos="1" side="tl" part="im_perf"/>
								<lit-tag v="past.m"/> <!-- TO DO FIX: need to get gender from previous word -->
								<clip pos="1" side="tl" part="nbr"/>
							</lu>
							<b/>
							<lu>
								<lit v="biti"/>
								<lit-tag v="vbser.cni"/>
							</lu>
						</chunk>
					</out>
				</when>
				<when>
					<test>
						<or>
				        <equal><clip pos="1" side="sl" part="temps"/><lit-tag v="prs"/></equal>
				        <equal><clip pos="1" side="sl" part="temps"/><lit-tag v="imp"/></equal>
				        <equal><clip pos="1" side="sl" part="temps"/><lit-tag v="pri"/></equal>
			         	</or>
			         </test>	
					<out>
						<chunk name="VerbSedanjik">
						<tags>
		          			<tag><lit-tag v="verb_prs"/></tag>
		        		</tags>
							<lu>
								<clip pos="1" side="tl" part="lemh"/>
								<clip pos="1" side="tl" part="a_verb"/>
								<clip pos="1" side="tl" part="im_perf"/>
								<lit-tag v="pres"/>
								<clip pos="1" side="tl" part="persona"/>
								<clip pos="1" side="tl" part="nbr"/>
							</lu>
						</chunk>
					</out>
				</when>
				<when>
					<test>
				        <equal><clip pos="1" side="sl" part="temps"/><lit-tag v="inf"/></equal>
			         </test>	
					<out>
						<chunk name="VerbInfinitive">
						<tags>
		          			<tag><lit-tag v="verb_inf"/></tag>
		        		</tags>
							<lu>
								<clip pos="1" side="tl" part="lemh"/>
								<clip pos="1" side="tl" part="a_verb"/>
								<clip pos="1" side="tl" part="im_perf"/>
								<lit-tag v="inf"/>
							</lu>
						</chunk>
					</out>
				</when>
				<when>
					<test>
				        <equal><clip pos="1" side="sl" part="temps"/><lit-tag v="ger"/></equal>
			         </test>	
					<out>
						<chunk name="VerbGerund">
						<tags>
		          			<tag><lit-tag v="verb_ger"/></tag>
		        		</tags>
		        		<lu>
							<clip pos="1" side="tl" part="lemh"/>
							<clip pos="1" side="tl" part="a_verb"/>
							<clip pos="1" side="tl" part="im_perf"/>
							<lit-tag v="supn"/>
						</lu>
		        		<!-- TO DO FIX: Output nothing -> Avendo l'opportunita ... -> S priložnostjo ...? -->
						</chunk>
					</out>
				</when>
				<when>
					<test>
				        <equal><clip pos="1" side="sl" part="temps"/><lit-tag v="fti"/></equal>
			         </test>	
					<out>
						<chunk name="VerbPrihodnjik">
						<tags>
		          			<tag><lit-tag v="verb_fti"/></tag>
		        		</tags>
		        		<lu>
							<lit v="biti"/>
							<lit-tag v="vbser.fut"/>
							<clip pos="1" side="tl" part="persona"/>
							<clip pos="1" side="tl" part="nbr"/>
						</lu>
						<b/>
							<lu>
								<clip pos="1" side="tl" part="lemh"/>
								<clip pos="1" side="tl" part="a_verb"/>
								<clip pos="1" side="tl" part="im_perf"/>
								<lit-tag v="past.m"/>
								<clip pos="1" side="tl" part="nbr"/>
							</lu>
						</chunk>
					</out>
				</when>				           
				<otherwise>
					<out>
						<chunk name="VerbWhole">
							<tags>
		              			<tag><lit-tag v="verb_whole"/></tag>
		            		</tags>
		            		<lu>
								<clip pos="1" side="tl" part="whole"/>
							</lu>
						</chunk>
					</out>
				</otherwise>
			</choose>
		</action>
	</rule>

	<rule comment="ADJ"> <!-- ITA ima samo osnovnike -->
      <pattern>
		<pattern-item n="adj"/>
      </pattern>
		<action>
			<out>
				<chunk name="adjchunk">
					<tags>
              			<tag><lit-tag v="adv"/></tag>
            		</tags>
					<lu>
						<clip pos="1" side="tl" part="lemh"/>
						<clip pos="1" side="tl" part="a_adj"/>
						<clip pos="1" side="tl" part="gen"/>
						<clip pos="1" side="tl" part="nbr"/>
						<lit-tag v="nom.ind"/>
					</lu>
				</chunk>
			</out>
		</action>
	</rule>

</section-rules>

