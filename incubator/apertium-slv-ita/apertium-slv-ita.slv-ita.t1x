			             <lit-tag v="m"/>
			           </equal>
			         </test>	
					<out>
						<chunk name="Pr_n1">
						<tags>
		          			<tag><lit-tag v="Pr_n1"/></tag>
		        		</tags>
							<lu>
								<clip pos="2" side="tl" part="lemh"/>
								<clip pos="2" side="tl" part="a_verb"/>
								<clip pos="2" side="tl" part="im_perf"/>
								<lit-tag v="pres.neg"/>
								<clip pos="2" side="tl" part="persona"/>
								<clip pos="2" side="tl" part="nbr"/>
							</lu>
							<b/>
							<lu>
								<clip pos="4" side="tl" part="lemh"/>
								<clip pos="4" side="tl" part="a_nom"/>
								<lit-tag v="ma"/>
								<clip pos="4" side="tl" part="nbr"/>
								<lit-tag v="gen"/>
							</lu>
						</chunk>
					</out>
				</when>
				<otherwise>
					<out>
						<chunk name="Pr_n2">
							<tags>
		              			<tag><lit-tag v="Pr_n2"/></tag>
		            		</tags>
							<lu>
								<clip pos="2" side="tl" part="lemh"/>
								<clip pos="2" side="tl" part="a_verb"/>
								<clip pos="2" side="tl" part="im_perf"/>
								<lit-tag v="pres.neg"/>
								<clip pos="2" side="tl" part="persona"/>
								<clip pos="2" side="tl" part="nbr"/>
							</lu>
							<b/>
							<lu>
								<clip pos="4" side="tl" part="lemh"/>
								<clip pos="4" side="tl" part="a_nom"/>
					           	<clip pos="4" side="tl" part="gen"/>
								<clip pos="4" side="tl" part="nbr"/>
								<lit-tag v="gen"/>
							</lu>
						</chunk>
					</out>
				</otherwise>
			</choose>
		</action>
	</rule>
	
	<rule comment="rule: sta dormendo -> stare (vblex) dormire (vblex.ger) -> spi (vblex.imperf.pres.p3.sg)">
      <pattern>
		<pattern-item n="verb"/>
	    <pattern-item n="verb_ger"/>
      </pattern>
		<action>
			<out>
				<chunk name="Verb_verbGer">
					<tags>
              			<tag><lit-tag v="Verb_verbGer"/></tag>
            		</tags>
					<lu>
						<clip pos="2" side="tl" part="lemh"/>
						<clip pos="2" side="tl" part="a_verb"/>
						<clip pos="2" side="tl" part="im_perf"/>
						<lit-tag v="pres"/>
						<clip pos="1" side="tl" part="persona"/>
						<clip pos="1" side="tl" part="nbr"/>
					</lu>
				</chunk>
			</out>
		</action>
	</rule>
	
	<rule comment="Questo -> Ta :: prn.itg.*.* - prn.dem.PERSONA.NBR.nom">
      <pattern>
		<pattern-item n="prn"/>
      </pattern>
		<action>
			<out>
				<chunk name="PRN.ITG">
					<tags>
              			<tag><lit-tag v="prn_itq"/></tag>
            		</tags>
					<lu>
						<clip pos="1" side="tl" part="lemh"/>
						<clip pos="1" side="tl" part="a_prn"/>
						<clip pos="1" side="tl" part="gen"/>
						<clip pos="1" side="tl" part="nbr"/>
						<lit-tag v="nom"/>

					</lu>
				</chunk>
			</out>
