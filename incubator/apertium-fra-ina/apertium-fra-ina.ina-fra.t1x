    </rule>

 <rule comment="RULE: conjugate NOM CNJ NOM CNJ NOM">
          <pattern>
        <pattern-item n="nom"/>
        <pattern-item n="cnj"/>
        <pattern-item n="nom"/>
        <pattern-item n="cnj"/>
        <pattern-item n="nom"/>

      </pattern>

      <action>  
          <call-macro n="nouns_macro">
