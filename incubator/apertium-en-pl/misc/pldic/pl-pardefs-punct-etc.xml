<?xml version="1.0" encoding="UTF-8"?>
<pardefs>
   <!-- punctuation etc. -->
   <pardef n="guionet">
      <e>
        <p>
          <l>-</l>
          <r>-<s n="guio"/></r>
        </p>
      </e>
    </pardef>
    <pardef n="coma">
      <e>
        <p>
          <l>,</l>
          <r>,<s n="cm"/></r>
        </p>
      </e>
    </pardef>
    <pardef n="cometa">
      <e>
         <re>[’'‘]</re>
         <p>
           <l/>
           <r><s n="apos"/></r>
         </p>
      </e>
    </pardef>
    <pardef n="lquote">
      <e>
        <p>
          <l>„</l>
          <r><s n="lquot"/></r>
        </p>
      </e>
      <e r="LR">
        <p>
          <l>«</l>
          <r><s n="lquot"/></r>
        </p>
      </e>
      <e r="LR">
        <p>
          <l>,,</l>
          <r><s n="lquot"/></r>
        </p>
      </e>
    </pardef>
    <pardef n="rquote">
      <e>
        <p>
          <l>”</l>
          <r><s n="rquot"/></r>
        </p>
      </e>
      <e r="LR">
        <p>
          <l>»</l>
          <r><s n="rquot"/></r>
        </p>
      </e>
      <e r="LR">
        <p>
          <l>''</l>
          <r><s n="rquot"/></r>
        </p>
      </e>
    </pardef>
    <pardef n="separa">
       <e>
         <re>[.\?;:!]</re>
         <p>
           <l/>
           <r><s n="sent"/></r>
         </p>
       </e>
    </pardef>
     <!-- Umm... neither Polish nor English use this, so is it necessary? -->
    <pardef n="separ">
       <e>
        <re>[¿¡]</re>
        <p>
          <l/>
          <r><s n="lquest"/></r>
        </p>
       </e>
    </pardef>
    <pardef n="paresq">
      <e>
        <re>[\(\[]</re>
        <p>
          <l/>
          <r><s n="lpar"/></r>
        </p>
      </e>
    </pardef>
    <pardef n="pardret">
      <e>
        <re>[\)\]]</re>
        <p>
          <l/>
         <r><s n="rpar"/></r>
        </p>
      </e>
    </pardef>
</pardefs>