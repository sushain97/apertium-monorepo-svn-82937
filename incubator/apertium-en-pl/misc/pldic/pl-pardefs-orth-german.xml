<?xml version="1.0" encoding="UTF-8"?>
<pardefs>
    <!-- Orthographic pardefs for German words -->
    <pardef n="ss_ß">
      <e r="LR" c="Prefer ss, but it's acceptable in some cases: http://www.rjp.pl/?mod=uchwaly&amp;id=4">
        <p>
          <l>ss</l>
          <r>ß</r>
        </p>
      </e>
      <e>
        <p>
          <l>ß</l>
          <r>ß</r>
        </p>
      </e>
    </pardef>
    <pardef n="ß_ss">
      <e>
        <p>
          <l>ss</l>
          <r>ss</r>
        </p>
      </e>
      <e r="LR">
        <p>
          <l>ß</l>
          <r>ss</r>
        </p>
      </e>
    </pardef>
  
