<rule comment="REGLA: aor">
      <pattern>
        <pattern-item n="prc_perf"/>
        <pattern-item n="vaux-aor"/>
      </pattern>
      <action>

<choose>
   <when>
      
      <test><equal><clip pos="1" side="tl" part="a_pers_nbr"/> 
