<?xml version="1.0" encoding="UTF-8"?>
<transfer default="chunk">
  <section-def-cats>
    <def-cat n="nom">
      <cat-item tags="n.*"/>
    </def-cat>
    <def-cat n="aor">
      <cat-item tags="v.*.aor.*"/>
    </def-cat>
<def-cat n="past">
      <cat-item tags="v.*.past.*"/>
    </def-cat>

    <def-cat n="sent">
      <cat-item tags="sent"/>
    </def-cat>

 <def-cat n="fut">
      <cat-item tags="v.*.fut.*"/>
    </def-cat>

<def-cat n="prc_impf">
      <cat-item tags="v.*.prc_impf"/>
    </def-cat>

<def-cat n="fut_plan">
      <cat-item tags="vaux.fut_plan.*"/>
    </def-cat>

<def-cat n="prc_vol">
      <cat-item tags="v.*.prc_vol.*"/>
    </def-cat>

<def-cat n="gpr_past">
      <cat-item tags="v.*.gpr_past.*"/>
    </def-cat>

<def-cat n="vaux-aor">
      <cat-item tags="vaux.aor.*"/>
    </def-cat>

<def-cat n="prc_perf">
      <cat-item tags="v.*.prc_perf"/>
    </def-cat>





<def-cat n="ifi">
      <cat-item tags="vaux.ifi.*"/>
    </def-cat>

<def-cat n="pres">
      <cat-item tags="vaux.pres.*"/>
    </def-cat>

<def-cat n="баста">
      <cat-item lemma="баста" tags="vaux.*"/>
    </def-cat>

<def-cat n="кел">
      <cat-item lemma="кел" tags="vaux.*"/>
    </def-cat>

<def-cat n="adj">
      <cat-item  tags="adj.*"/>
      <cat-item  tags="adj"/>
    </def-cat>

<def-cat n="cop">
      <cat-item  tags="cop.*"/>
    </def-cat>



<def-cat n="yat_pres">
<cat-item lemma="жат" tags="vaux.pres.*"/>
<cat-item lemma="тұр" tags="vaux.pres.*"/>
<cat-item lemma="жүр" tags="vaux.pres.*"/>

</def-cat>

 <def-cat n="otyr_pres">
<cat-item lemma="отыр" tags="vaux.pres.*"/>
</def-cat>

<def-cat n="cop-neg">
<cat-item lemma="е" tags="cop.neg.*"/>
</def-cat>

   
  </section-def-cats>

  <section-def-attrs>
    <def-attr n="a_cas">
      <attr-item tags="nom"/>
    </def-attr>

<def-attr n="evid">
      <attr-item tags="aor.evid"/>
      <attr-item tags="dub.aor"/>
    </def-attr>



<def-attr n="tenses">
      <attr-item tags="ifi"/>
      <attr-item tags="fut"/>
     <attr-item tags="past"/>
<attr-item tags="aor"/>
    </def-attr>

<def-attr n="neg">
      <attr-item tags="neg"/>
      
    </def-attr>





<def-attr n="V_trans">
     <attr-item tags="iv"/>
     <attr-item tags="tv"/>

   </def-attr>
  <def-attr n="POS">
     <attr-item tags="v"/>
     <attr-item tags="n"/>
     <attr-item tags="adj"/>
     <attr-item tags="adv"/>
   </def-attr>

    <def-attr n="a_pers_nbr">
      <attr-item tags="p1.sg"/>
      <attr-item tags="p2.sg"/>
      <attr-item tags="p3.sg"/>
      <attr-item tags="p1.pl"/>
      <attr-item tags="p2.pl"/>
      <attr-item tags="p3.pl"/>
 <attr-item tags="p2.frm.sg"/> 
    </def-attr>
<def-attr n="a_aspect">
      <attr-item tags="aor"/>
      <attr-item tags="prc_perf"/>
     <attr-item tags="fut"/>
<attr-item tags="prc_impf"/>
<attr-item tags="prog"/>
<attr-item tags="past"/>
    </def-attr>
  </section-def-attrs>

  <section-def-vars>
    <def-var n="number"/>
  </section-def-vars>

  <section-def-macros>
    <def-macro n="test" npar="1">
          <let>
            <var n="number"/>
            <lit v=""/>
          </let>
    </def-macro>
  </section-def-macros>

  <section-rules>
    <rule comment="REGLA: AOR">
      <pattern>
        <pattern-item n="aor"/>
      </pattern>
      <action>

<choose>
   <when>

     <test><equal><clip pos="1" side="tl" part="a_pers_nbr"/> 
                             <lit-tag v="p2.frm.sg"/></equal></test>
  
     <let><clip pos="1" side="tl" part="a_pers_nbr"/><lit-tag v="p2.pl"/></let>
    </when>
</choose>
<let>
<clip pos="1" side="tl" part="a_aspect"/><lit-tag v="prog"/>
</let>
        <let>
          <clip pos="1" side="tl" part="a_pers_nbr"/>
          <concat><lit v="+i"/><lit-tag v="cop.aor"/><clip pos="1" side="tl" part="a_pers_nbr"/></concat>
        </let>
        <out>
          <chunk name="v" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>




  <rule comment="REGLA: past">
      <pattern>
        <pattern-item n="past"/>
      </pattern>
      <action>

<choose>
   <when>
       <!-- IF: a_pers_nbr IS p2.frm.sg -->
      <test><equal><clip pos="1" side="tl" part="a_pers_nbr"/> 
                             <lit-tag v="p2.frm.sg"/></equal></test>
        <!-- THEN: LET a_pers_nbr BE p2.pl -->
      <let><clip pos="1" side="tl" part="a_pers_nbr"/><lit-tag v="p2.pl"/></let>
    </when>
</choose>
<let>
<clip pos="1" side="tl" part="a_aspect"/><lit-tag v="past"/>
</let>
        <let>
          <clip pos="1" side="tl" part="a_pers_nbr"/>
          <concat><lit v="+i"/><lit-tag v="cop.aor"/><clip pos="1" side="tl" part="a_pers_nbr"/></concat>
        </let>
        <out>
          <chunk name="v" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

<rule comment="REGLA: fut plan">
      <pattern>
        <pattern-item n="prc_impf"/>
        <pattern-item n="fut_plan"/>
      </pattern>
      <action>

<choose>
   <when>
       <!-- IF: a_pers_nbr IS p2.frm.sg -->
      <test><equal><clip pos="1" side="tl" part="a_pers_nbr"/> 
                             <lit-tag v="p2.frm.sg"/></equal></test>
        <!-- THEN: LET a_pers_nbr BE p2.pl -->
      <let><clip pos="1" side="tl" part="a_pers_nbr"/><lit-tag v="p2.pl"/></let>
    </when>
</choose>
<let>
<clip pos="1" side="tl" part="a_aspect"/><lit-tag v="fut"/>
</let>
        <let>
          <clip pos="1" side="tl" part="a_pers_nbr"/>
          <concat><lit v="+i"/><lit-tag v="cop.aor"/><clip pos="1" side="tl" part="a_pers_nbr"/></concat>
        </let>
        <out>
          <chunk name="v" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>





<rule comment="REGLA: fut">
       <pattern>
        <pattern-item n="fut"/>    
      </pattern>
      <action>
        <let>
          <clip pos="1" side="tl" part="a_pers_nbr"/>
          <concat><lit v="+i"/><lit-tag v="cop.aor"/><clip pos="1" side="tl" part="a_pers_nbr"/></concat>
        </let>
        <out>
          <chunk name="v" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
