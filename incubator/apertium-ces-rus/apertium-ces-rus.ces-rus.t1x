    <rule comment="REGLA: NE JEST (není)">
      <pattern>
        <pattern-item n="ne"/>
        <pattern-item n="je"/>
      </pattern>
      <action>
        <out>
          <chunk name="net" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SENT"/></tag>
            </tags>
            <mlu>
              <lu>
                <clip pos="1" side="tl" part="whole"/>
              </lu>
              <lu>
                <clip pos="2" side="tl" part="whole"/>
              </lu>
            </mlu>
          </chunk>
        </out>
      </action>
    </rule>


    <rule comment="REGLA: cop-pres">
      <pattern>
        <pattern-item n="cop-pres"/>
      </pattern>
      <action>
        <out>
          <chunk name="cop" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="COP"/></tag>
            </tags>
            <b/>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="REGLA: past-verb">
      <pattern>
        <pattern-item n="past-verb"/>
      </pattern>
      <action>
        <choose>
          <when><test>
            <equal><clip pos="1" side="tl" part="a_nbr"/><lit-tag v="pl"/></equal></test>
            <let><clip pos='1' side='tl' part='a_gen'/><lit-tag v='mfn'/></let></when>
        </choose>
        <out>
          <chunk name="verb" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="REGLA: SENT">
      <pattern>
        <pattern-item n="sent"/>
      </pattern>
      <action>
        <out>
          <chunk name="sent" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SENT"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>


    <rule comment="REGLA: DET">
      <pattern>
        <pattern-item n="deter"/>
      </pattern>
      <action>
        <choose>
          <when><test>
            <and><not><equal><clip pos="1" side="tl" part="a_cas"/><lit-tag v="acc"/></equal></not>
            <not><equal><clip pos="1" side="tl" part="det_type"/><lit-tag v="pos"/></equal></not></and></test>
              <out>
              <chunk name="det" case="caseFirstWord">
                <tags>
                  <tag><lit-tag v="SD"/></tag>
                </tags>
                <lu>
                  <clip pos="1" side="tl" part="lem"/>
                  <clip pos="1" side="tl" part="a_det"/>
                  <clip pos="1" side="tl" part="det_type"/>
                  <clip pos="1" side="tl" part="a_gen"/>
                  <lit-tag v="an"/>
                  <clip pos="1" side="tl" part="a_nbr"/>
                  <clip pos="1" side="tl" part="a_cas"/>
                </lu>
              </chunk>
            </out>
          </when>
        </choose>
      </action>
    </rule>

    <rule comment="REGLA: PRN">
      <pattern>
        <pattern-item n="pro_no_prs"/>
      </pattern>
      <action>
        <choose>
          <when><test>
            <and><not><equal><clip pos="1" side="tl" part="a_cas"/><lit-tag v="acc"/></equal></not>
            <not><equal><clip pos="1" side="tl" part="prn_type"/><lit-tag v="neg"/></equal></not></and></test>
              <out>
              <chunk name="prn" case="caseFirstWord">
                <tags>
                  <tag><lit-tag v="SP"/></tag>
                </tags>
                <lu>
                  <clip pos="1" side="tl" part="lem"/>
                  <clip pos="1" side="tl" part="a_prn"/>
                  <clip pos="1" side="tl" part="prn_type"/>
                  <clip pos="1" side="tl" part="a_gen"/>
                  <lit-tag v="an"/>
                  <clip pos="1" side="tl" part="a_nbr"/>
                  <clip pos="1" side="tl" part="a_cas"/>
                </lu>
              </chunk>
            </out>
          </when>
        </choose>
      </action>
    </rule>



    <rule comment="REGLA: NUM">
      <pattern>
        <pattern-item n="num"/>
      </pattern>
      <action>
        <out>
          <chunk name="num" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SNUM"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lem"/>
              <clip pos="1" side="tl" part="a_num"/>
              <lit-tag v="mfn"/>
              <clip pos="1" side="tl" part="a_nbr"/>
              <clip pos="1" side="tl" part="a_cas"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="REGLA: VBHAVER TV">
      <pattern>
        <pattern-item n="vbhaver"/>
      </pattern>
      <action>
        <out>
          <chunk name='vbhaver_tv' case="caseFirstWord">
            <tags>
              <tag><lit-tag v="VBHTV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lem"/>
              <lit-tag v="vblex"/>
              <clip pos="1" side="tl" part="v_asp"/>
              <clip pos="1" side="tl" part="v_trans"/>
