    <def-macro n="f_bcond" npar="2"><!--Per mirar si el blanc conté o no format. Cal posar aquesta funció en les regles en què desaparegui alguna paraula, per tal de decidir si el blanc de la paraula eliminada s'ha d'eliminar o conservar. Si conté format cal conservar-la, si no en conté cal eliminar-la perquè no apareguin dos espais seguits. -->
      <choose>
	<when>
	  <test>
	    <not>
	      <equal>
		<b pos="1"/>
		<lit v=" "/>
	      </equal>
	    </not>
	  </test>
	  <out>
	    <b pos="1"/>
	  </out>
	</when>     	    
      </choose>
    </def-macro>


 <def-macro n="firstWord" npar="1" c="modified by mginesti 11/nov/10">
      <choose>
	<when>
	  <test> 
	    <equal>
	      <clip pos="1" side="sl" part="enmajuscula"/>
	      <lit v=""/>
	    </equal>
	  </test>
	  <choose>
	    <when>
	      <test>
		<equal> 
		  <var n="EOS"/>
		  <lit v="true"/>
		</equal> 
	      </test>
	      <modify-case>
		<clip pos="1" side="tl" part="lem"/>
		<lit v="aa"/>
	      </modify-case>
	      <let>
		<var n="caseFirstWord"/>
		<lit v="Aa"/>
	      </let>
	    </when>
	    <otherwise> 
	      <let>
		<var n="caseFirstWord"/>
		<lit v="aa"/>
	      </let>
	    </otherwise>
	  </choose>
	</when>
	<otherwise><!--when it is np or n.acr or rom -->
	  <choose>
	    <when>
	      <test>
		<equal> 
		  <var n="EOS"/>
		  <lit v="true"/>
		</equal> 
	      </test>
	      <let>
		<var n="caseFirstWord"/>
		<lit v="Aa"/>
	      </let>
	    </when>
	    <otherwise> 
	      <let>
		<var n="caseFirstWord"/>
		<lit v="aa"/>
	      </let>
	    </otherwise>
	  </choose> 
	</otherwise>
      </choose>
      <let>
	<var n="EOS"/>
	<lit v="false"/>
      </let>
    </def-macro>




 <def-macro n="tipo_posposicion" npar="1">
      <choose>
	<when>
	  <test>
	    <or>
	      <equal>		
		<clip pos="1" side="sl" part="lem"/> 
		<lit v="en"/>
	      </equal>
	      <equal>		
		<clip pos="1" side="sl" part="lem"/> 
		<lit v="ko"/>
	      </equal>
	      <equal>		
		<clip pos="1" side="sl" part="a_post"/> 
		<lit-tag v="ko"/>
	      </equal>
	    </or>
	  </test>
	  <let>
	    <var n="tipo_post"/>
	    <lit-tag v="GEN"/>
	  </let>
	</when>

	<when>
	  <test>
	    <equal>
	      <clip pos="1" side="sl" part="lem"/> 
	      <lit v="k"/>
	    </equal>
	  </test>
	  <let>
	    <var n="tipo_post"/>
	    <lit-tag v="ERG"/>
	  </let>
	</when>
	<when>
	  <test>
	      <equal>
		<clip pos="1" side="sl" part="lem"/> 
		<lit v="ik"/>
	      </equal>
	  </test>
	  <let>
	    <var n="tipo_post"/>
	    <lit-tag v="PART"/>
	  </let>
	</when>
	<when>
	  <test><!--MG: en eu_bis creo que no existe -ekin, sólo -agaz -->
	      <equal>
		<clip pos="1" side="sl" part="lem"/> 
		<lit v="ekin"/>
	      </equal>
	  </test>
	  <let>
	    <var n="tipo_post"/>
	    <lit-tag v="EKIN"/>
	  </let>
	</when>
	<when>
	  <test>
	      <equal>
		<clip pos="1" side="sl" part="lem"/> 
		<lit v="gaz"/>
	      </equal>
	  </test>
	  <let>
	    <var n="tipo_post"/>
	    <lit-tag v="AGAZ"/>
	  </let>
	</when>
	<when>
	  <test>
	    <equal>
	      <clip pos="1" side="sl" part="a_gra"/> 
	      <lit-tag v="gra"/>
	    </equal>
	  </test>
	  <let>
	    <var n="tipo_post"/>
	    <lit-tag v="GRA"/>
	  </let>
	</when>
	<otherwise>
	  <let>
	    <var n="tipo_post"/>
	    <lit-tag v="POST"/>
	  </let>
	</otherwise>
      </choose>
</def-macro>



<def-macro n="tipo_adv" npar="1">
