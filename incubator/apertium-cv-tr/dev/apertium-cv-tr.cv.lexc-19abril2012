LEXICON POS-INFL ! After possession

%<pl%>:%>се%{М%} CASE ;
CASE ;

LEXICON POS-NUM             	! Posession for numerals

%<px1pl%>:%>с%{Ӑ%}м%{Ӑ%}р CASE ;
%<px2pl%>:%>с%{Ӑ%}р CASE ;
%<px3sp%>:%>ш%{А%}р CASE ;

LEXICON POS		! Posession

%<px1sg%>:%>%{Ӑ%}м POS-INFL ;
%<px2sg%>:%>%{У%}%{н2%} POS-INFL ;
%<px3sp%>:%>%{ӗ%}%{н%} POS-INFL ;
%<px1pl%>:%>%{Ӑ%}м%>%{Ӑ%}р POS-INFL ;
%<px2pl%>:%>%{Ӑ%}р POS-INFL ;

LEXICON POS-И		! Posession for Chuvash nouns with final и

%<px1sg%>:%>й%{Ӑ%}м CASE ;
%<px2sg%>:%>й CASE-PX2SG ;
%<px3sp%>:%>й CASE-PX3SP ;
%<px1pl%>:%>й%{Ӑ%}м%>%{Ӑ%}р CASE ;
%<px2pl%>:%>й%{Ӑ%}р CASE ;
POS-PL-И ;

LEXICON POS-А		! Posession for Chuvash nouns with final а

%<px1sg%>:а%>м CASE ;
%<px2sg%>:%> CASE-PX2SG ;
%<px3sp%>:%> CASE-PX3SP-И ;
%<px1pl%>:а%>м%>%{Ӑ%}р CASE ;
%<px2pl%>:а%>р CASE ;
POS-PL-А ;

LEXICON POS-О		! Posession for Chuvash nouns with final о

%<px1sg%>:о%>м CASE ;
%<px2sg%>:%> CASE-PX2SG-BACK ;
%<px3sp%>:%> CASE-PX3SP-И ;
%<px1pl%>:о%>м%>%{Ӑ%}р CASE ;
%<px2pl%>:о%>р CASE ;
POS-PL-О ;

LEXICON POS-ЬЯ		! Posession for Chuvash nouns with final ья

%<px1sg%>:ья%>м CASE ;
%<px2sg%>:ь%> CASE-PX2SG-Я ;
%<px3sp%>:%>й CASE-PX3SP-И ;
%<px1pl%>:ья%>м%>%{Ӑ%}р CASE ;
%<px2pl%>:%>йӑр CASE ;
POS-PL-ЬЯ ;

LEXICON POS-Ӑ_Ӗ

%<px1sg%>:%>%{Ӑ%}м CASE ;
%<px2sg%>:%> CASE-PX2SG ;
%<px3sp%>:%> CASE-PX3SP ;
%<px1pl%>:%>%{Ӑ%}м%>%{Ӑ%}р CASE ;
%<px2pl%>:%>%{Ӑ%}р CASE ;
POS-PL ;

!LEXICON POS-FRONT	! Posession for Chuvash nouns forcing front vowels
!
!%<px1sg%>:%>ӗм CASE ;
!%<px2sg%>:%> CASE-PX2SG-FRONT ;
!%<px3sp%>:%> CASE-PX3SP ;
!%<px1pl%>:%>ӗм%>ӗр CASE ;
!%<px2pl%>:%>ӗр CASE ;
!POS-PL-FRONT ;

LEXICON POS-BACK	! Posession for Chuvash nouns forcing back vowels

%<px1sg%>:%>ӑм CASE ;
%<px2sg%>:%> CASE-PX2SG-BACK ;
%<px3sp%>:%> CASE-PX3SP ;
%<px1pl%>:%>ӑм%>ӑр CASE ;
%<px2pl%>:%>ӑр CASE ;
POS-PL-BACK ;

LEXICON POS-Е		! Posession for Chuvash nouns with final е

%<px1sg%>:е%>м CASE ;
%<px2sg%>:%> CASE-PX2SG-FRONT ;
%<px3sp%>:%> CASE-PX3SP-И ;
%<px1pl%>:е%>м%>ӗр CASE ;
%<px2pl%>:е%>р CASE ;
POS-PL-Е ;

LEXICON POS-АТТЕ

%<px1sg%>:тте%>м CASE ;
%<px2sg%>:ҫ%> CASE-PX2SG-FRONT ;
%<px3sp%>:шш%> CASE-PX3SP ;
%<px1pl%>:тте%>м%>ӗр CASE ;
%<px2pl%>:тте%>р CASE ;
POS-PL-АТТЕ ;

LEXICON POS-АННЕ

%<px1sg%>:нне%>м CASE ;
%<px2sg%>:нн%> CASE-PX2SG-FRONT ;
%<px3sp%>:м%>ӑш CASE-PX3SP ;
%<px1pl%>:нне%>м%>ӗр CASE ;
%<px2pl%>:нне%>р CASE ;
POS-PL-АННЕ ;

LEXICON POS-ПИЧЧЕ

%<px1sg%>:нне%>м CASE ;
%<px2sg%>:нн%> CASE-PX2SG-FRONT ;
%<px3sp%>:м%>ӑш CASE-PX3SP ;
%<px1pl%>:нне%>м%>ӗр CASE ;
%<px2pl%>:нне%>р CASE ;
POS-PL-ПИЧЧЕ ;

LEXICON POS-МУЧИ

%<px1sg%>:нне%>м CASE ;
%<px2sg%>:нн%> CASE-PX2SG-FRONT ;
%<px3sp%>:м%>ӑш CASE-PX3SP ;
%<px1pl%>:нне%>м%>ӗр CASE ;
%<px2pl%>:нне%>р CASE ;
POS-PL-МУЧИ ;

LEXICON POS-КИНЕМЕЙ

%<px1sg%>:нне%>м CASE ;
%<px2sg%>:нн%> CASE-PX2SG-FRONT ;
%<px3sp%>:м%>ӑш CASE-PX3SP ;
%<px1pl%>:нне%>м%>ӗр CASE ;
%<px2pl%>:нне%>р CASE ;
POS-PL-КИНЕМЕЙ ;

LEXICON POS-КУКАҪИ

%<px1sg%>:нне%>м CASE ;
%<px2sg%>:нн%> CASE-PX2SG-FRONT ;
%<px3sp%>:м%>ӑш CASE-PX3SP ;
%<px1pl%>:нне%>м%>ӗр CASE ;
%<px2pl%>:нне%>р CASE ;
POS-PL-КУКАҪИ ;

LEXICON POS-VCӐ	! Posession for Chuvash nouns with final Vowel + Cons + ӑ

%<px1sg%>:%{dup%}%>ӑм CASE ;
%<px2sg%>:%{dup%}%> CASE-PX2SG-BACK ;
%<px3sp%>:%{dup%}%> CASE-PX3SP-И ;
%<px1pl%>:%{dup%}%>ӑм%>ӑр CASE ;
%<px2pl%>:%{dup%}%>ӑр CASE ;
POS-PL-DUP ;

LEXICON POS-VCӖ	! Posession for Chuvash nouns with final Vowel + Cons + ӗ

%<px1sg%>:%{dup%}%>ӗм CASE ;
%<px2sg%>:%{dup%}%> CASE-PX2SG-FRONT ;
%<px3sp%>:%{dup%}%> CASE-PX3SP-И ;
%<px1pl%>:%{dup%}%>ӗм%>ӗр CASE ;
%<px2pl%>:%{dup%}%>ӗр CASE ;
POS-PL-DUP ;

!LEXICON POS-CC		! Posession for Chuvash nous with final (double) consonant
!
!%<px1sg%>:%>%{Ӑ%}м CASE ;
!%<px2sg%>:%> CASE-PX2SG ;
!%<px3sp%>:%{dup%}%> CASE-PX3SP-И ;
!%<px1pl%>:%>%{Ӑ%}м%>%{Ӑ%}р CASE ;
!%<px2pl%>:%>%{Ӑ%}р CASE ;
!POS-PL-CC ;

LEXICON N-INFL 		! Chuvash nouns, standard paradigm

CASE ;
!%<n%>%<pl%>:%>се CASE-PL ;
%<pl%>:%>се%{М%} CASE ;
POS ;

LEXICON N-INFL-DUP

:%{dup%}%{Ӑ%} N ;

LEXICON N

%<n%>: N-INFL ; 

LEXICON N-INFL-Й 	! Chuvash nouns with final й

%<n%>:%{й%} N-INFL ;

LEXICON NP-Top

%<np%>%<top%>: CASE ;

LEXICON NP-Ant-F

%<np%>%<ant%>%<f%>: CASE ;

LEXICON NP-Ant-M

%<np%>%<ant%>%<m%>: CASE ;

LEXICON NP-Cog-F

%<np%>%<cog%>%<f%>: CASE ;

LEXICON NP-Cog-M

%<np%>%<cog%>%<m%>: CASE ;

LEXICON NP-Ant-F-И	! Chuvash nouns with final и

%<np%>%<ant%>%<f%>: CASE-NOM ;
%<np%>%<ant%>%<f%>: CASE-GEN_DAT-И ;
%<np%>%<ant%>%<f%>: CASE-LOC_ABL ;
%<np%>%<ant%>%<f%>: CASE-INS_PRIV_TERM ;

LEXICON N-INFL-RUS-NONOM 	! Russian nouns, standard paradigm without nominative

%<n%>: CASE-OBL-BACK ;
%<n%>%<pl%>:%>се CASE-PL ;
%<n%>: POS-BACK ;

LEXICON N-INFL-RUS 	! Russian nouns, standard paradigm

%<n%>: CASE-NOM ;
N-INFL-RUS-NONOM ;

LEXICON N-INFL-RUS_A-UNSTR 	! Russian nouns with final unstressed а

%<n%>:а CASE-NOM ;
%<n%>: CASE-Ӑ-OBL ;
%<n%>%<pl%>:ӑ%>се CASE-PL ;
%<n%>: POS-BACK ;

LEXICON N-INFL-RUS_Е-UNSTR 	! Russian nouns with final unstressed е

%<n%>:е CASE-NOM ;
%<n%>: CASE-Ӗ-OBL ;
%<n%>%<pl%>:ӗ%>се CASE-PL ;
%<n%>: POS-BACK ;

LEXICON NP-Ant-M-RUS 		! Russian proper nouns (masculine)

%<np%>%<ant%>%<m%>: CASE-NOM ;
%<np%>%<ant%>%<m%>: CASE-OBL-BACK ;

LEXICON NP-Ant-F-RUS 		! Russian proper nouns (feminine)

%<np%>%<ant%>%<f%>: CASE-NOM ;
%<np%>%<ant%>%<f%>: CASE-OBL-BACK ;

LEXICON NP-Ant-M-RUS_Я-UNSTR 	! Russian proper nouns with final unstressed я (masculine)

%<np%>%<ant%>%<m%>:я CASE-NOM ;
%<np%>%<ant%>%<m%>: CASE-Ӑ-OBL-NP-RUS ;

LEXICON NP-Ant-F-RUS_Я-UNSTR 	! Russian proper nouns with final unstressed я (feminine)

%<np%>%<ant%>%<f%>:я CASE-NOM ;
%<np%>%<ant%>%<f%>: CASE-Ӑ-OBL-NP-RUS ;

LEXICON NP-Ant-M-RUS_A-UNSTR 	! Russian proper nouns with final unstressed а (masculine)

%<np%>%<ant%>%<m%>:а CASE-NOM ;
%<np%>%<ant%>%<m%>: CASE-Ӑ-OBL ;

LEXICON NP-Ant-F-RUS_A-UNSTR 	! Russian proper nouns with final unstressed а (feminine)

%<np%>%<ant%>%<f%>:а CASE-NOM ;
%<np%>%<ant%>%<f%>: CASE-Ӑ-OBL ;

