LEXICON VINFL-COMMON

%<inf%>:%>м%{А%} K ;
%<inf%>:%>м%{А%}шк%{Ӑ%}н K ;		  ! Dir/LR
%<past%>:%>н%{Ӑ%} K ;
%<neg%>%<past%>:%>м%{А%}%>н K ;

%<pres%>:%>%{А%} V-PERS-PRES ;
%<neg%>%<pres%>:%>м%{А%} V-PERS-PRES-NEG ;
%<evid%>:%>р V-PERS-EVID ;
%<neg%>%<evid%>:%>м%{А%}%>р V-PERS-EVID ;
%<pih%>:%>%{А%}т V-PERS-PIH ;
%<neg%>%<pih%>:%>м%{А%}с%>т V-PERS-PIH-NEG ;
%<fut%>:%> V-PERS-FUT ;
%<neg%>%<fut%>:%>м%> V-PERS-FUT ;
%<cond%>:%> V-PERS-COND ;
%<neg%>%<cond%>:%>м%> V-PERS-COND ;
%<opt%>:%> V-PERS-OPT ;
%<neg%>%<opt%>:%>м%> V-PERS-OPT ;
%<imp%>: V-PERS-IMP ;

