Multichar_Symbols

%<n%>              ! Существительное · Noun
%<v%>              ! Глагол
%<num%>            ! Число · Numeral
%<pred%>           ! · Predicative
%<adj%>            ! Прилагательное
%<adv%>            ! Наречие
%<prn%>            ! Местомение
%<det%>            !
%<cop%>            !
%<part%>           !

%<subst%>          ! Субстативные
%<attr%>           ! 

%<np%>             ! 
%<ant%>            ! 
%<cog%>            ! 
%<top%>            ! 

%<nom%>            ! Инемительный / Основной · Nominative
%<gen%>            ! Родительный / Притяжательный · Genitive
%<dat%>            ! Дательный · Dative
%<loc%>            ! Местный · Locative
%<abl%>            ! Исходный · Ablative
%<ins%>            ! Творительный · Instrumental
%<abe%>           ! Лишительный · Privative
%<term%>           ! · Terminative
%<fin%>            ! Прпичинно-целевой · Final / Purposive
%<all%>            ! Аллатив · Allative / Directive


%<pers%>           !
%<ind%>            !
%<dem%>            !
%<itg%>            !
%<ref%>            !
%<qst%>            !

%<comp%>           !

%<ord%>            ! Порядковые
%<dist%>           ! Дистрибутивные
%<coll%>           ! Собирательные

%<m%>              ! Мужской род
%<f%>              ! Женский род

%<p1%>             ! 
%<p2%>             ! 
%<p3%>             ! 

%<iv%>             ! 
%<tv%>             ! 

%<inf%>            ! Инфинитив
%<ifi%>            ! 
%<pres%>           ! Настоящее время
%<evid%>           ! Прошедщее очевидное время
%<past%>           ! Прошедщее неочевидное время
%<pih%>            ! Прошедщее многократное время
%<fut%>            ! Будущее время
%<cond%>           ! Сослагательное наклонение
%<imp%>            ! Повлелительное наклонение
%<opt%>            !

%<cnjcoo%>         ! Союз

%<p1%>             !
%<p2%>             !
%<p3%>             !

%<px1sg%>          ! Possessive, first person singular
%<px2sg%>          ! Possessive, second person singular
%<px3sp%>          ! Possessive, third person
%<px1pl%>          ! Possessive, first person plural
%<px2pl%>          ! Possessive, second person plural

%<sg%>             !
%<pl%>             ! Множенственное число · Plural

%<sent%>           ! Sentence marker
%<guio%>           ! Hyphen
%<cm%>             ! Comma
%<apos%>           ! Apostrophe
%<rquot%>          ! Quote marker (right hand side) 
%<lquot%>          ! Quote marker (left hand side) 
%<rpar%>           ! Parenthetical marker (right hand side) 
%<lpar%>           ! Parenthetical marker (left hand side) 



%{Ӑ%}              ! Archiphoneme 'Ӑ' ('ӑ' or 'ӗ')
%{А%}              ! Archiphoneme 'А' ('a' or 'e')
%{У%}              ! Archiphoneme 'У' ('ӳ' or 'у')
!%{Е%}             ! Archiphoneme 'Е' ('и' or 'е')
%{ч%}              ! Either 'ч' or nothing
%{ӗ%}              ! Archiphoneme 'ӗ' for px3sp (conditionates the previous consonant)
%{й%}              ! Final й of the stem, which can be changed to я, ю or е 
%{ь%}              ! 3p sg suffix: вула·т·ь - пӗл·ӗт·
%{Р%}              ! р%{А%}/т%{А%}/ч%{А%}, р%{А%}н/т%{А%}н/ч%{А%}н
%{Т%}              ! т%{А%}, т%{А%}н
%{М%}              ! сем/сен
%{н%}              ! н that appears in dative after vowels and after <px3sg> before some cases
%{н2%}             ! н that appears after <px2sg> before some cases

%{w%}              ! Holds a place for в in у/ӳ > ӑв/ӗв
%{dup%}            ! indicates that the previous consonant must be duplicated

%>                 ! · Morpheme boundary
% 


LEXICON Root
!Regexp ; 

Particles ; ! :(
Punctuation ;
Pronouns ;
Determiners ;
Numerals ;
Proper_Names ;
Conjunctions ;
Adjectives ;
Adverbs ;
Nouns ;
Verbs ;

LEXICON K 

%+и%<qst%>:%-и # ;
# ;

LEXICON COP

# ;
%+ӗ%<cop%>%<ifi%>:%>%{ч%}чӗ K;
K ;

LEXICON CASE-NOM

%<nom%>: COP ;

LEXICON CASE-GEN_DAT

%<gen%>:%>%{Ӑ%}н COP ;
%<dat%>:%>%{А%} COP ;

LEXICON CASE-GEN_DAT-А_Е

%<gen%>:%>н COP ;
%<dat%>:%>н%{А%} COP ;

LEXICON CASE-GEN_DAT-О

%<gen%>:%>н COP ;
%<dat%>:%>на COP ;

LEXICON CASE-GEN_DAT-И

%<gen%>:%>н # ;
%<gen%>:%>йӗн # ; 	! Dir/LR
%<dat%>:%>е # ;

LEXICON CASE-GEN_DAT-FRONT

%<gen%>:%>ӗн # ;
%<dat%>:%>е # ;

LEXICON CASE-GEN_DAT-BACK

%<gen%>:%>ӑн # ;
%<dat%>:%>а # ;

LEXICON CASE-GEN_DAT-PX2SG

%<gen%>:%{У%}%>н # ;
%<dat%>:%{Ӑ%}в%>%{А%} # ;

LEXICON CASE-GEN_DAT-PX2SG-FRONT

%<gen%>:ӳ%>н # ;
%<dat%>:ӗв%>е # ;

LEXICON CASE-GEN_DAT-PX2SG-BACK

%<gen%>:у%>н # ;
%<dat%>:ӑв%>а # ;

LEXICON CASE-GEN_DAT-PX2SG-Я

%<gen%>:ю%>н # ;
%<dat%>:%{Ӑ%}в%>%{А%} # ;		! ?????

LEXICON CASE-GEN_DAT-PX3SP

%<gen%>:%{ӗ%}%>н # ;
%<dat%>:%>не # ;

LEXICON CASE-GEN_DAT-PX3SP-И

%<gen%>:%>н # ;
%<gen%>:%>йӗн # ; 	! Dir/LR
%<dat%>:%>не # ;	! не / е ??

LEXICON CASE-LOC_ABL

%<loc%>:%>%{Р%}%{А%} # ;
%<abl%>:%>%{Р%}%{А%}н # ;

LEXICON CASE-LOC_ABL-BACK

%<loc%>:%>ра # ;
%<abl%>:%>ран # ;

! LEXICON CASE-LOC_ABL-Л_Ль_Н
! 
! %<loc%>:%>р%{А%} # ;
! %<abl%>:%>р%{А%}н # ;
! %<abl%>:%>т%{А%}н # ;	! Dir/LR

LEXICON CASE-LOC_ABL-PX3SP

%<loc%>:%>нч%{А%} # ;
%<abl%>:%>нч%{А%}н # ;

LEXICON CASE-INS_PRIV_TERM
%<ins%>:%>п%{А%} # ;
%<abe%>:%>с%{Ӑ%}р # ;
%<term%>:%>ш%{Ӑ%}н # ;
%<all%>:%>%{А%}лл%{А%} # ;	! WARNING: Doesn't work yet: the first %{А%} falls in words as каҫма or чӗлхе

LEXICON CASE-INS_PRIV_TERM-BACK
%<ins%>:%>па # ;
%<abe%>:%>сӑр # ;
%<term%>:%>шӑн # ;
%<all%>:%>алла # ;


!LEXICON CASE-OBL	! oblique cases
!
!CASE-GEN_DAT ;
!CASE-LOC_ABL ;
!CASE-INS_PRIV_TERM ;

LEXICON CASE-OBL-BACK

CASE-GEN_DAT-BACK ;
CASE-LOC_ABL-BACK ;
CASE-INS_PRIV_TERM-BACK ;

LEXICON CASE

%<nom%>: COP ;
%<gen%>:%{w%}%>%{Ӑ%}н COP ;
%<dat%>:%{w%}%>%{н%}%{А%} COP ;
%<loc%>:%>%{Р%}%{А%} COP ;
%<abl%>:%>%{Р%}%{А%}н COP ;
%<abl%>:%>%{Т%}%{А%}н COP ;
%<ins%>:%>п%{А%} COP ;
%<abe%>:%>с%{Ӑ%}р COP ;
%<term%>:%>ш%{Ӑ%}н COP ;
%<all%>:%{w%}%>алла COP ;


! LEXICON CASE-Ь	! cases for Chuvash nouns with final ь
! 
! %<n%>:ь CASE-NOM ;
! %<n%>: CASE-GEN_DAT-FRONT ;
! !!! NO LOC/ABL???
! %<n%>:ь CASE-INS_PRIV_TERM ;
! %<n%>%<pl%>:ь%>се CASE-PL ;
! 
!LEXICON CASE-У	! cases for Chuvash nouns with final у
!
!:у CASE-NOM ;
!:ӑв CASE-GEN_DAT ;
!:у CASE-LOC_ABL ;
!:у CASE-INS_PRIV_TERM ;
!
!LEXICON CASE-Ӳ	! cases for Chuvash nouns with final ӳ
!
!:ӳ CASE-NOM ;
!:ӗв CASE-GEN_DAT ;
!:ӳ CASE-LOC_ABL ;
!:ӳ CASE-INS_PRIV_TERM ;

LEXICON CASE-Ӑ_Ӗ-OBL	! oblique cases for Chuvash nouns with final ӑ

CASE-GEN_DAT ;
:%{Ӑ%} CASE-LOC_ABL ;
:%{Ӑ%} CASE-INS_PRIV_TERM ;

LEXICON CASE-Ӑ-OBL	! oblique cases for Chuvash nouns with final ӑ

CASE-GEN_DAT-BACK ;
:ӑ CASE-LOC_ABL ;
:ӑ CASE-INS_PRIV_TERM ;

LEXICON CASE-Ӑ-OBL-NP-RUS	! oblique cases for Chuvash loans from Russian with final ӑ

%<gen%>:%>ӑн # ;
%<dat%>:%>ӑна # ;
:ӑ CASE-LOC_ABL ;
:ӑ CASE-INS_PRIV_TERM ;


LEXICON CASE-Ӗ-OBL	! oblique cases for Chuvash nouns with final ӗ

CASE-GEN_DAT-FRONT ;
:ӗ CASE-LOC_ABL ;
:ӗ CASE-INS_PRIV_TERM ;

LEXICON CASE-VCӐ	! cases for Chuvash nouns with final ӑ

:ӑ CASE-NOM ;
:%{dup%} CASE-GEN_DAT-BACK ;
:ӑ CASE-LOC_ABL ;
:ӑ CASE-INS_PRIV_TERM ;

LEXICON CASE-VCӖ	! cases for Chuvash nouns with final ӗ

:ӗ CASE-NOM ;
:%{dup%} CASE-GEN_DAT-FRONT ;
:ӗ CASE-LOC_ABL ;
:ӗ CASE-INS_PRIV_TERM ;

LEXICON CASE-PX2SG	! cases for Chuvash nouns with px2sg suffix

:%{У%} CASE-NOM ;
CASE-GEN_DAT-PX2SG ;
:%{У%} CASE-LOC_ABL ;
:%{У%} CASE-INS_PRIV_TERM ;

LEXICON CASE-PX2SG-FRONT	! cases for Chuvash nouns with px2sg suffix forcing front vowels

:ӳ CASE-NOM ;
CASE-GEN_DAT-PX2SG-FRONT ;
:ӳ CASE-LOC_ABL ;
:ӳ CASE-INS_PRIV_TERM ;

LEXICON CASE-PX2SG-BACK		! cases for Chuvash nouns with px2sg suffix forcing back vowels

:у CASE-NOM ;
CASE-GEN_DAT-PX2SG-BACK ;
:у CASE-LOC_ABL ;
:у CASE-INS_PRIV_TERM ;

LEXICON CASE-PX2SG-Я	! cases for Chuvash nouns with px2sg suffix

:ю CASE-NOM ;
CASE-GEN_DAT-PX2SG-Я ;
:ю CASE-LOC_ABL ;
:ю CASE-INS_PRIV_TERM ;

LEXICON CASE-PX3SP	! cases for Chuvash nouns with px3sp suffix ӗ

:%{ӗ%} CASE-NOM ;
CASE-GEN_DAT-PX3SP ;
:%{ӗ%} CASE-LOC_ABL-PX3SP ;
:%{ӗ%} CASE-INS_PRIV_TERM ;

LEXICON CASE-PX3SP-И	! cases for Chuvash nouns with px3sp suffix и

:и CASE-NOM ;
:и CASE-GEN_DAT-PX3SP-И ;		! double genitive form, as genitive in -и ?
:и CASE-LOC_ABL-PX3SP ;
:и CASE-INS_PRIV_TERM ;

LEXICON CASE-PL		! case endings after plural suffix
 
%<nom%>:м%> # ;
%<gen%>:%>н # ;
%<dat%>:н%>е # ;
%<loc%>:н%>че # ;
%<abl%>:н%>чен # ;
%<ins%>:м%>пе # ;
%<abe%>:м%>сӗр # ;
%<term%>:м%>шӗн # ;

LEXICON POS-PL		! Posession plural

%<px1sg%>%<pl%>:%>%{Ӑ%}м%>се CASE-PL ;
%<px2sg%>%<pl%>:%>%{У%}%>се CASE-PL ;
%<px3sp%>%<pl%>:%>%{ӗ%}%>се CASE-PL ;
%<px1pl%>%<pl%>:%>%{Ӑ%}м%>%{Ӑ%}р%>се CASE-PL ;
%<px2pl%>%<pl%>:%>%{Ӑ%}р%>се CASE-PL ;

!LEXICON POS-PL-FRONT		! Posession plural
!
!%<px1sg%>%<pl%>:%>ӗм%>се CASE-PL ;
!%<px2sg%>%<pl%>:%>ӳ%>се CASE-PL ;
!%<px3sp%>%<pl%>:%>%{ӗ%}%>се CASE-PL ;
!%<px1pl%>%<pl%>:%>ӗм%>ӗр%>се CASE-PL ;
!%<px2pl%>%<pl%>:%>ӗр%>се CASE-PL ;

LEXICON POS-PL-BACK		! Posession plural

%<px1sg%>%<pl%>:%>ӑм%>се CASE-PL ;
%<px2sg%>%<pl%>:%>у%>се CASE-PL ;
%<px3sp%>%<pl%>:%>%{ӗ%}%>се CASE-PL ;
%<px1pl%>%<pl%>:%>ӑм%>ӑр%>се CASE-PL ;
%<px2pl%>%<pl%>:%>ӑр%>се CASE-PL ;

LEXICON POS-PL-DUP		! Posession plural

%<px1sg%>%<pl%>:%{dup%}%>%{Ӑ%}м%>се CASE-PL ;
%<px2sg%>%<pl%>:%{dup%}%>%{У%}%>се CASE-PL ;
%<px3sp%>%<pl%>:%{dup%}%>и%>се CASE-PL ;
%<px1pl%>%<pl%>:%{dup%}%>%{Ӑ%}м%>%{Ӑ%}р%>се CASE-PL ;
%<px2pl%>%<pl%>:%{dup%}%>%{Ӑ%}р%>се CASE-PL ;

LEXICON POS-PL-И		! Posession plural

%<px1sg%>%<pl%>:%>й%{Ӑ%}м%>се CASE-PL ;
%<px2sg%>%<pl%>:%>й%{У%}%>се CASE-PL ;
%<px3sp%>%<pl%>:%>йӗ%>се CASE-PL ;
%<px1pl%>%<pl%>:%>й%{Ӑ%}м%>%{Ӑ%}р%>се CASE-PL ;
%<px2pl%>%<pl%>:%>й%{Ӑ%}р%>се CASE-PL ;

LEXICON POS-PL-А		! Posession plural

%<px1sg%>%<pl%>:а%>м%>се CASE-PL ;
%<px2sg%>%<pl%>:%>у%>се CASE-PL ;
%<px3sp%>%<pl%>:%>и%>се CASE-PL ;
%<px1pl%>%<pl%>:а%>м%>%{Ӑ%}р%>се CASE-PL ;
%<px2pl%>%<pl%>:а%>р%>се CASE-PL ;

LEXICON POS-PL-О		! Posession plural

%<px1sg%>%<pl%>:о%>м%>се CASE-PL ;
%<px2sg%>%<pl%>:%>у%>се CASE-PL ;
%<px3sp%>%<pl%>:%>и%>се CASE-PL ;
%<px1pl%>%<pl%>:о%>м%>%{Ӑ%}р%>се CASE-PL ;
%<px2pl%>%<pl%>:о%>р%>се CASE-PL ;

LEXICON POS-PL-ЬЯ		! Posession plural

%<px1sg%>%<pl%>:ья%>м%>се CASE-PL ;
%<px2sg%>%<pl%>:ь%>ю%>се CASE-PL ;
%<px3sp%>%<pl%>:%>йи%>се CASE-PL ;
%<px1pl%>%<pl%>:ья%>м%>%{Ӑ%}р%>се CASE-PL ;
%<px2pl%>%<pl%>:ья%>р%>се CASE-PL ;

LEXICON POS-PL-Е		! Posession plural

%<px1sg%>%<pl%>:е%>м%>се CASE-PL ;
%<px2sg%>%<pl%>:%>ӳ%>се CASE-PL ;
%<px3sp%>%<pl%>:%>и%>се CASE-PL ;
%<px1pl%>%<pl%>:е%>м%>%{Ӑ%}р%>се CASE-PL ;
%<px2pl%>%<pl%>:е%>р%>се CASE-PL ;

LEXICON POS-PL-АТТЕ		! Posession plural

%<px1sg%>%<pl%>:тте%>м%>се CASE-PL ;
%<px2sg%>%<pl%>:ҫ%>ӳ%>се CASE-PL ;
%<px3sp%>%<pl%>:шш%>ӗ%>се CASE-PL ;
%<px1pl%>%<pl%>:тте%>м%>%{Ӑ%}р%>се CASE-PL ;
%<px2pl%>%<pl%>:тте%>р%>се CASE-PL ;

LEXICON POS-PL-АННЕ		! Posession plural

%<px1sg%>%<pl%>:нне%>м%>се CASE-PL ;
%<px2sg%>%<pl%>:нн%>ӳ%>се CASE-PL ;
%<px3sp%>%<pl%>:м%>ӑшӗ%>се CASE-PL ;
%<px1pl%>%<pl%>:нне%>м%>%{Ӑ%}р%>се CASE-PL ;
%<px2pl%>%<pl%>:нне%>р%>се CASE-PL ;

LEXICON POS-PL-ПИЧЧЕ		! Posession plural

%<px1sg%>%<pl%>:нне%>м%>се CASE-PL ;
%<px2sg%>%<pl%>:нн%>ӳ%>се CASE-PL ;
%<px3sp%>%<pl%>:м%>ӑшӗ%>се CASE-PL ;
%<px1pl%>%<pl%>:нне%>м%>%{Ӑ%}р%>се CASE-PL ;
%<px2pl%>%<pl%>:нне%>р%>се CASE-PL ;

LEXICON POS-PL-МУЧИ		! Posession plural

%<px1sg%>%<pl%>:нне%>м%>се CASE-PL ;
%<px2sg%>%<pl%>:нн%>ӳ%>се CASE-PL ;
%<px3sp%>%<pl%>:м%>ӑшӗ%>се CASE-PL ;
%<px1pl%>%<pl%>:нне%>м%>%{Ӑ%}р%>се CASE-PL ;
%<px2pl%>%<pl%>:нне%>р%>се CASE-PL ;

LEXICON POS-PL-КИНЕМЕЙ		! Posession plural

%<px1sg%>%<pl%>:нне%>м%>се CASE-PL ;
%<px2sg%>%<pl%>:нн%>ӳ%>се CASE-PL ;
%<px3sp%>%<pl%>:м%>ӑшӗ%>се CASE-PL ;
%<px1pl%>%<pl%>:нне%>м%>%{Ӑ%}р%>се CASE-PL ;
%<px2pl%>%<pl%>:нне%>р%>се CASE-PL ;

LEXICON POS-PL-КУКАҪИ		! Posession plural

%<px1sg%>%<pl%>:нне%>м%>се CASE-PL ;
%<px2sg%>%<pl%>:нн%>ӳ%>се CASE-PL ;
%<px3sp%>%<pl%>:м%>ӑшӗ%>се CASE-PL ;
