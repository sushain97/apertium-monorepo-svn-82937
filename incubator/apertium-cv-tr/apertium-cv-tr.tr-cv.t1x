    <rule comment="regla: daha adj → adj.comp">
      <pattern>
        <pattern-item n="daha"/>
        <pattern-item n="adj"/>
      </pattern>
      <action>
        <out>
          <chunk name="a" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SA"/></tag>
            </tags>
            <lu>
              <get-case-from pos="1">
                <clip pos="2" side="tl" part="lem"/>
              </get-case-from>
              <lit-tag v="adj.comp"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>
 
    <rule comment="regla: detind">
      <pattern>
        <pattern-item n="detind"/>
      </pattern>
      <action>
        <out>
          <chunk name="det" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="DET.IND"/></tag>
            </tags>
            <lu><lit v=""/></lu>
          </chunk>
        </out>
      </action>
    </rule>

 
    <rule comment="regla: npant ve npant → npant-INS npant">
      <pattern>
        <pattern-item n="npant"/>
        <pattern-item n="ve"/>
        <pattern-item n="npant"/>
      </pattern>
      <action>
        <call-macro n="convposs1">
          <with-param pos="1"/>
        </call-macro>
        <call-macro n="convcase1">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="n" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_nom"/>
              <clip pos="1" side="tl" part="a_gen"/>
              <clip pos="1" side="tl" part="a_nbr"/>
              <lit-tag v="ins"/>
              <clip pos="1" side="tl" part="lemq"/>
            </lu>
            <b pos="1"/>
            <lu>
              <clip pos="3" side="tl" part="lemh"/>
              <clip pos="3" side="tl" part="a_nom"/>
              <clip pos="3" side="tl" part="a_gen"/>
              <clip pos="3" side="tl" part="a_nbr"/>
              <lit-tag v="nom"/>
              <clip pos="3" side="tl" part="lemq"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>
 
    <!-- if an adjective is translated to a noun, izafet must be added -->
    <rule comment="regla: adj nom → nom nom.px3sp">
      <pattern>
        <pattern-item n="adj"/>
        <pattern-item n="noun"/>
      </pattern>
      <action>
        <call-macro n="convposs1">
          <with-param pos="2"/>
        </call-macro>
        <call-macro n="convcase1">
          <with-param pos="2"/>
        </call-macro>
      <choose>
        <when>
          <test>
            <equal><clip pos="1" side="tl" part="a_adj"/><lit-tag v="adj"/></equal>
          </test>
          <!-- adjective translated to adjective: no problem -->
        <out>
          <chunk name="a" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SA"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
          <b pos="1"/>
          <chunk name="n">
            <tags>
              <tag><lit-tag v="SN"/></tag>
              <tag><var n="падеж"/></tag>
              <tag><var n="poss"/></tag>
            </tags>
            <lu>
              <clip pos="2" side="tl" part="lemh"/>
              <clip pos="2" side="tl" part="a_nom"/>
              <clip pos="2" side="tl" part="a_gen"/>
              <var n="poss"/>
              <clip pos="2" side="tl" part="a_nbr"/>
              <var n="падеж"/>
              <clip pos="2" side="tl" part="lemq"/>
            </lu>
          </chunk>
        </out>
        </when>
        <otherwise>
          <!-- adjective translated to noun: add izafet -->
        <out>
          <chunk name="n" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
              <tag><var n="падеж"/></tag>
              <tag><lit-tag v="px3sp"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_nom"/>
              <clip pos="1" side="tl" part="a_pos"/>
              <clip pos="1" side="tl" part="a_nbr"/>
              <lit-tag v="nom"/>
              <clip pos="1" side="tl" part="lemq"/>
            </lu>
            <b pos="1"/>
            <lu>
              <clip pos="2" side="tl" part="lemh"/>
              <clip pos="2" side="tl" part="a_nom"/>
              <clip pos="2" side="tl" part="a_gen"/>
              <lit-tag v="px3sp"/>
              <clip pos="2" side="tl" part="a_nbr"/>
              <var n="падеж"/>
              <clip pos="2" side="tl" part="lemq"/>
            </lu>
          </chunk>
        </out>
        </otherwise>
      </choose>
      </action>
    </rule>
 
    <rule comment="regla: bir nom → nom">
      <pattern>
        <pattern-item n="bir"/>
        <pattern-item n="noun"/>
      </pattern>
      <action>
        <call-macro n="convposs1">
          <with-param pos="2"/>
        </call-macro>
        <call-macro n="convcase1">
          <with-param pos="2"/>
        </call-macro>
        <out>
          <chunk name="n" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
            </tags>
            <lu>
              <clip pos="2" side="tl" part="lemh"/>
              <clip pos="2" side="tl" part="a_nom"/>
              <clip pos="2" side="tl" part="a_gen"/>
              <var n="poss"/>
              <clip pos="2" side="tl" part="a_nbr"/>
              <var n="падеж"/>
              <clip pos="2" side="tl" part="lemq"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

 
    <rule comment="regla: noun">
      <pattern>
        <pattern-item n="noun"/>
      </pattern>
      <action>
        <call-macro n="convposs1">
          <with-param pos="1"/>
        </call-macro>
        <call-macro n="convcase1">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="n" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
              <tag><var n="падеж"/></tag>
              <tag><var n="poss"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_nom"/>
              <clip pos="1" side="tl" part="a_gen"/>
              <!-- <lit-tag v="3"/> -->
              <var n="poss"/>
              <clip pos="1" side="tl" part="a_nbr"/>
              <!-- <lit-tag v="2"/> dóna problemes per loc.attr -->
              <var n="падеж"/>
              <clip pos="1" side="tl" part="lemq"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="regla: pron">
      <pattern>
        <pattern-item n="pron"/>
      </pattern>
      <action>
        <call-macro n="convposs1">
          <with-param pos="1"/>
        </call-macro>
        <call-macro n="convcase1">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="pron" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_prn"/>
              <clip pos="1" side="tl" part="a_pers"/>
              <clip pos="1" side="tl" part="a_gen"/>
              <clip pos="1" side="tl" part="a_nbr"/>
              <var n="poss"/>
              <var n="падеж"/>
              <clip pos="1" side="tl" part="lemq"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>


    
    <rule comment="regla: cop-pres">
      <pattern>
        <pattern-item n="cop-pres"/>
      </pattern>
      <action>
        <out>
          <chunk name="cop" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <lit v=""/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>


    
    <rule comment="regla: v_infin">
      <pattern>
        <pattern-item n="v_infin"/>
      </pattern>
      <action>
        <call-macro n="vinconj1">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="inf" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_verb"/>
              <clip pos="1" side="tl" part="a_trans"/>
              <clip pos="1" side="tl" part="a_deriv"/>
              <clip pos="1" side="tl" part="a_neg"/>
              <clip pos="1" side="tl" part="a_temps"/>
              <clip pos="1" side="tl" part="a_pos"/>
              <clip pos="1" side="tl" part="a_nbr"/>
              <clip pos="1" side="tl" part="a_cas"/>
              <var n="падеж"/>
              <clip pos="1" side="tl" part="lemq"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="regla: v_fin">
      <pattern>
        <pattern-item n="v_fin"/>
      </pattern>
      <action>
        <call-macro n="vconj1">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="v" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_verb"/>
              <clip pos="1" side="tl" part="a_trans"/>
              <clip pos="1" side="tl" part="a_deriv"/>
              <clip pos="1" side="tl" part="a_neg"/>
              <clip pos="1" side="tl" part="a_temps"/>
              <clip pos="1" side="tl" part="a_pers"/>
              <clip pos="1" side="tl" part="a_nbr"/>
              <clip pos="1" side="tl" part="lemq"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="regla: v_fin mi_pers">
      <pattern>
        <pattern-item n="v_fin"/>
        <pattern-item n="mi_pers"/>
      </pattern>
      <action>
        <call-macro n="convpers1">
          <with-param pos="2"/>
        </call-macro>
        <call-macro n="vconj1">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <chunk name="v" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SV"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
