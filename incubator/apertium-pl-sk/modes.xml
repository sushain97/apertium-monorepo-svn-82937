        <file name="pl-sk.autobil.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-pl-sk.pl-sk.t2x"/>
        <file name="pl-sk.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-pl-sk.pl-sk.t3x"/>
        <file name="pl-sk.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="pl-sk.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="pl-sk.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

