  <section-rules>

    <rule comment="REGLA: num guio num time-loc">
      <pattern>
        <pattern-item n="num"/>
        <pattern-item n="guio"/>
        <pattern-item n="num"/>
        <pattern-item n="time-loc"/>
      </pattern>
      <action>
        <choose>
          <when><test><not> <equal caseless="yes">
                        <clip pos="4" side="tl" part="lemh"/><lit v="год"/>
                      </equal></not>
                </test>
                <let><clip pos="4" side="tl" part="a_case"/><lit-tag v="prp"/></let>
          </when>
        </choose>
        <out>
          <chunk name="num-n" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN.LOC"/></tag>
              <tag><clip pos="4" side="sl" part="a_case"/></tag>
            </tags>
            <lu>
              <lit v="в"/>
              <lit-tag v="pr"/>
            </lu>
            <b/>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
            <b pos="1"/>
            <lu>
              <clip pos="2" side="tl" part="whole"/>
            </lu>
            <b pos="2"/>
            <lu>
              <clip pos="3" side="tl" part="whole"/>
            </lu>
            <b pos="3"/>
            <lu>
              <clip pos="4" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>


    <rule comment="REGLA: num time-loc">
      <pattern>
        <pattern-item n="num"/>
        <pattern-item n="time-loc"/>
      </pattern>
      <action>
        <choose>
          <when><test><not> <equal caseless="yes">
                        <clip pos="2" side="tl" part="lemh"/><lit v="год"/>
                      </equal></not>
                </test>
                <let><clip pos="2" side="tl" part="a_case"/><lit-tag v="prp"/></let>
          </when>
        </choose>
        <out>
          <chunk name="num-n" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN.LOC"/></tag>
              <tag><clip pos="2" side="sl" part="a_case"/></tag>
            </tags>
            <lu>
              <lit v="в"/>
              <lit-tag v="pr"/>
            </lu>
            <b/>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
            <b pos="1"/>
            <lu>
              <clip pos="2" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="REGLA: num n-nom">
      <pattern>
        <pattern-item n="num"/>
        <pattern-item n="n-nom"/>
      </pattern>
      <action>

        <let><var n="syn-func"/><lit-tag v="ABS"/></let>
        <choose>
          <when><test><or>
                        <equal><clip pos="2" side="tl" part="a_func"/><lit-tag v="@SUBJ→"/></equal>
                        <equal><clip pos="2" side="tl" part="a_func"/><lit-tag v="@←SUBJ"/></equal>
                      </or>
                </test>
                <let><var n="syn-func"/><lit-tag v="SUBJ"/></let>
          </when>
        </choose>
        <let><clip pos="2" side="tl" part="a_func"/><lit v=""/></let>
        <let><clip pos="1" side="tl" part="a_case"/><lit-tag v="nom"/></let>
        <let><clip pos="1" side="tl" part="a_gen_gd"/><clip pos="2" side="tl" part="a_gen"/></let>
        <let><clip pos="1" side="tl" part="a_anim"/><lit-tag v="an"/></let>

        <choose>
          <when>
            <test><or><equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="один"/></equal>
                      <equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="только один"/></equal>
                  </or></test>
            <let><clip pos="1" side="tl" part="a_gen_gd"/><clip pos="2" side="tl" part="a_gen"/></let>
          </when>
          <when>
            <test><or><equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="два"/></equal>
                      <equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="три"/></equal>
                      <equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="четыре"/></equal></or>
            </test>
            <let><clip pos="2" side="tl" part="a_case"/><lit-tag v="gen"/></let>
          </when>
          <otherwise>
            <let><clip pos="2" side="tl" part="a_nbr"/><lit-tag v="pl"/></let>
            <let><clip pos="2" side="tl" part="a_case"/><lit-tag v="gen"/></let>
          </otherwise>
        </choose>
        <out>
          <chunk name="num-n" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN.ABS"/></tag>
              <tag><clip pos="1" side="sl" part="a_case"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
            <b/>
            <lu>
              <clip pos="2" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="REGLA: num n-obl">
      <pattern>
        <pattern-item n="num"/>
        <pattern-item n="n-obl"/>
      </pattern>
      <action>
        <let><clip pos="2" side="tl" part="a_func"/><lit v=""/></let>
        <choose>
          <when>
            <test><or><equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="один"/></equal>
                      <equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="только один"/></equal>
                  </or></test>
            <let><clip pos="1" side="tl" part="a_gen_gd"/><clip pos="2" side="tl" part="a_gen"/></let>
          </when>
          <when>
            <test><or><equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="два"/></equal>
                      <equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="три"/></equal>
                      <equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="четыре"/></equal></or>
            </test>
            <let><clip pos="2" side="tl" part="a_case"/><lit-tag v="gen"/></let>
          </when>
          <otherwise>
            <let><clip pos="2" side="tl" part="a_nbr"/><lit-tag v="pl"/></let>
            <let><clip pos="2" side="tl" part="a_case"/><lit-tag v="gen"/></let>
          </otherwise>
        </choose>

        <out>
          <chunk name="z-n" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN.OBL"/></tag>
              <tag><clip pos="2" side="tl" part="a_gen"/></tag>
              <tag><clip pos="2" side="tl" part="a_anim"/></tag>
              <tag><clip pos="2" side="tl" part="a_nbr"/></tag>
              <tag><lit-tag v="CD"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
            <b pos="1"/>
            <lu>
              <clip pos="2" side="tl" part="lemh"/>
              <clip pos="2" side="tl" part="a_nom"/>
              <clip pos="2" side="tl" part="a_gen"/>
              <clip pos="2" side="tl" part="a_anim"/>
              <clip pos="2" side="tl" part="a_nbr"/>
              <lit-tag v="6"/>
              <clip pos="2" side="tl" part="lemq"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="REGLA: num n-erg">
      <pattern>
        <pattern-item n="num"/>
        <pattern-item n="n-erg"/>
      </pattern>
      <action>
        <let><clip pos="2" side="tl" part="a_case"/><lit-tag v="nom"/></let>
        <let><clip pos="1" side="tl" part="a_case"/><lit-tag v="nom"/></let>
        <let><clip pos="1" side="tl" part="a_gen_gd"/><clip pos="2" side="tl" part="a_gen"/></let>
        <let><clip pos="1" side="tl" part="a_anim"/><lit-tag v="an"/></let>

        <choose>
          <when>
            <test><equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="один"/></equal></test>
            <let><clip pos="1" side="tl" part="a_gen_gd"/><clip pos="2" side="tl" part="a_gen"/></let>
          </when>
          <when>
            <test><or><equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="два"/></equal>
                      <equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="три"/></equal>
                      <equal caseless="yes"><clip pos="1" side="tl" part="lemh"/><lit v="четыре"/></equal></or>
            </test>
            <let><clip pos="2" side="tl" part="a_case"/><lit-tag v="gen"/></let>
          </when>
          <otherwise>
            <let><clip pos="2" side="tl" part="a_nbr"/><lit-tag v="pl"/></let>
            <let><clip pos="2" side="tl" part="a_case"/><lit-tag v="gen"/></let>
          </otherwise>
        </choose>

        <let><clip pos="2" side="tl" part="a_func"/><lit v=""/></let>
        <out>
          <chunk name="num-n" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN.SUBJ"/></tag>
              <tag><clip pos="2" side="sl" part="a_case"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
            <b/>
            <lu>
              <clip pos="2" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>


    <rule comment="REGLA: preadj adj">
      <pattern>
        <pattern-item n="preadj"/>
        <pattern-item n="adj"/>
      </pattern>
      <action>
        <out>
          <chunk name="a-j" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SA"/></tag>
              <tag><lit-tag v="GD.AD.ND.CD"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
            <b pos="1"/>
            <lu>
              <clip pos="2" side="tl" part="lemh"/>
              <clip pos="2" side="tl" part="a_adj"/>
              <lit-tag v="2.3.4.5"/>
              <clip pos="2" side="tl" part="lemq"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="REGLA: adj">
      <pattern>
        <pattern-item n="adj"/>
      </pattern>
      <action>
        <let><clip pos="2" side="tl" part="a_case"/><lit-tag v="nom"/></let>
        <let><clip pos="1" side="tl" part="whole"/>
             <concat><clip pos="1" side="tl" part="lem"/>
                     <clip pos="1" side="tl" part="a_adj"/>
