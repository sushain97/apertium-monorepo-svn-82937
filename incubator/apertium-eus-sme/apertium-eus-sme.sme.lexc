! Personal pronouns
! -----------------


LEXICON MUNLEX
+Sg1: firstperspronsg ;
+Du1: firstpersprondu ;
+Pl1: perspronpl ;

LEXICON DONLEX
+Sg2: nonfirstperspronsg ;
+Du2: nonfirstpersprondu ;
+Pl2: perspronpl ;

LEXICON SONLEX
+Sg3: nonfirstperspronsg ;
+Du3: nonfirstpersprondu ;
+Pl3: perspronpl ;


!Case suffixes for 1st Sg, pointing to K lexicon
LEXICON firstperspronsg
+Nom:u%>n K ;
+Nom+Use/NG:o%>n K ; ! 
+Acc:u%> K ;
+Gen:u%> K ;
+Gen:u»# NAMAT ;
+Ill:u%>nnje K ;
+Loc:u%>s K ;
+Com:u%>jnna K ;
+Ess:u%>nin K ;

!Case suffixes for non-1st Sg, pointing to K lexicon
LEXICON nonfirstperspronsg
+Nom:o%>n K ;
+Acc:u%> K ;
+Gen:u%> K ;
+Gen:u»# NAMAT ;
+Ill:u%>tnje K ;
+Loc:u%>s K ;
+Com:u%>jnna K ;
+Ess:u%>nin K ;

!Case suffixes for 1st Du, pointing to K lexicon
LEXICON firstpersprondu
+Nom:oaj%> K ;
+Acc:unno%> K ;
+Gen:unno%> K ;
+Gen:unno»# NAMAT ;
+Ill:unnu%>jde K ;
+Loc:unno%>s K ;
+Com:unnu%>jn K ;
+Ess:unno%>n K ;

!Case suffixes for non-1st Du, pointing to K lexicon
LEXICON nonfirstpersprondu
+Nom:oaj%> K ;
+Acc:udno%> K ;
+Gen:udno%> K ;
+Acc+Use/Sub:utno%> K ; !
+Gen+Use/Sub:utno%> K ; !
+Gen:udno»# NAMAT ;
+Ill:udnu%>jde K ;
+Ill+Use/Sub:utnu%>jde K ; !
+Loc:udno%>s K ;
+Loc+Use/Sub:utno%>s K ; !
+Com:udnu%>jn K ;
+Com+Use/Sub:utnu%>jn K ; !
+Ess:udno%>n K ;
+Ess+Use/Sub:utno%>n K ; !

!Case suffixes for all Pl forms, pointing to K lexicon
LEXICON perspronpl
+Nom:i%>j K ;
+Acc:i%>n K ;
+Gen:i%>n K ;
+Gen:i%>n»# NAMAT ;
+Ill+Use/Sub:i%>nsij K ; !
+Ill:i%>djijde K ;
+Ill+Use/Sub:i%>nsiidda K ; 
+Loc:i%>s K ;
+Loc+Use/Sub:i%>js K ; ! !SOUTH
+Com:i%>ngujn K ;
+Ess:i%>nin K ;

LEXICON datpers
+Sg3:0 demcassg ;
+Pl3:0 demcaspl ;
+Ess:nin K ;




