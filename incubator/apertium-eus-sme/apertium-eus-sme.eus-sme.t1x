<?xml version="1.0" encoding="utf-8"?>
<transfer default="chunk">

<section-def-cats>
  <def-cat n="nom">
     <cat-item tags="N.*"/>
  </def-cat>
  
  <def-cat n="propnom">
     <cat-item tags="N.Prop.*"/>
  </def-cat>
  
  <def-cat n="verb_nom">
     <cat-item tags="V.Der_N.*"/>
  </def-cat>
  
  <def-cat n="verb_cont">
     <cat-item tags="V.Der_N.Cont.*"/>
  </def-cat>
  
  <def-cat n="verb_prc">
     <cat-item tags="V.Prc.*"/>
     <cat-item tags="V.Prc"/>
  </def-cat>
  
  <def-cat n="aux">
     <cat-item tags="V.Aux.*"/>
  </def-cat>
  
    <def-cat n="aux_pres">
     <cat-item tags="V.Aux.Ind.Prs.*"/>
  </def-cat>
  
  <def-cat n="vblex">
     <cat-item tags="vblex.*"/>
  </def-cat>
  
  <def-cat n="prn">
     <cat-item tags="Pron.*"/>
  </def-cat>
  <def-cat n="adj">
     <cat-item tags="A.*"/>
  </def-cat>
  <def-cat n="pr">
     <cat-item tags="pr"/>
  </def-cat>
  <def-cat n="det">
     <cat-item tags="det"/>
     <cat-item tags="det.*"/>
  </def-cat>
  <def-cat n="prefix-adj">
     <cat-item tags="pref.adj"/>
     <cat-item tags="pref.adj.*"/>
  </def-cat>
  <def-cat n="exp">
     <cat-item tags="part.exp"/>
  </def-cat>
</section-def-cats>

