     <attr-item tags="Du2"/>
     <attr-item tags="Du3"/>
     <attr-item tags="Pl1"/>
     <attr-item tags="Pl2"/>
     <attr-item tags="Pl3"/>
     <attr-item tags="NR_NI"/>
     <attr-item tags="NR_ZU"/>
     <attr-item tags="NR_HURA"/>
