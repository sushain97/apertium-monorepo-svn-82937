</section-def-attrs>

<section-def-vars>
  <def-var n="number"/>
  <def-var n="genere"/>
  <def-var n="Excase"/>
  <def-var n="Exsyn"/>
  <def-var n="Exsem"/>
  <def-var n="Cacase"/>
  <def-var n="Casyn"/>
