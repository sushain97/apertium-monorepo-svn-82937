  <def-attr n="mood">
    <attr-item tags="Ind"/>
    <attr-item tags="Imprt"/>
    <attr-item tags="Pot"/>
    <attr-item tags="Cond"/>
    <attr-item tags="Neg"/> 
    <attr-item tags="Neg.Ind"/> 
  </def-attr>

  <def-attr n="a_vrb">
