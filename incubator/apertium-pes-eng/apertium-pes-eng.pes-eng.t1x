	  <pattern-item n="c_adjsup"/>
    </pattern>
     <action>
	   <out>
         <chunk name="adjsup">
          <tags>
            <tag><lit-tag v="SADGSUP"/></tag>
            <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
          
		  </tags>
		  <lu>
        <lit v="the most"/>
        <lit-tag v="det"/>
        <lit-tag v="qnt"/>
		<lit-tag v="sp"/>
		</lu>
		 <b/>
		 <lu>
           <clip pos="1" side="tl" part="lem"/>
           <clip pos="1" side="tl" part="a_adj"/>
           <clip pos="1" side="tl" part="a_nbr"/>
        <lit-tag v="sint"/>
		 </lu>
        </chunk>
       </out>
    </action>
  </rule>
  
   <rule comment="142. number + noun"> 
    <pattern> 
      <pattern-item n="c_num"/>
	  <pattern-item n="nom"/>
    </pattern>
     <action>
       <out>
         <chunk name="num + noun">
          <tags>
            <tag><lit-tag v="SNUMN"/></tag>
            <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
          </tags>
         <lu>
		
		 <clip pos="1" side="tl" part="lem"/>
         <clip pos="1" side="tl" part="a_num"/>
		</lu>
		 <b/>
         <lu>
           <clip pos="2" side="tl" part="lem"/>
           <clip pos="2" side="tl" part="a_nom"/>
            <lit-tag v="pl"/>
		 </lu>
        </chunk>
       </out>
    </action>
  </rule>

  <rule comment="143. Pronoun"> 
    <pattern> 
      <pattern-item n="prn"/>
    </pattern>
     <action>
       <call-macro n="nbr-pers"><with-param pos="1"/></call-macro>
       <out>
         <chunk name="Pronoun">
          <tags>
            <tag><lit-tag v="SN"/></tag>
            <tag><clip pos="1" side="tl" part="a_nbr"/></tag>
          </tags>
         <lu>
           <clip pos="1" side="tl" part="lem"/>
           <clip pos="1" side="tl" part="a_prn"/>
           <clip pos="1" side="tl" part="a_subobj"/>
           <clip pos="1" side="tl" part="a_pers"/>
           <clip pos="1" side="tl" part="a_gender"/>
           <clip pos="1" side="tl" part="a_nbr" link-to="2"/>
         </lu>
        </chunk>
       </out>
    </action>
  </rule>

