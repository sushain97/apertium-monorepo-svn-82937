<section-def-macros>

  <def-macro n="nbr-pers" npar="1">
      <choose>
       <when>
         <test>
           <and>
            <equal>
              <clip pos="1" side="sl" part="a_pers"/>
              <lit-tag v="p2"/>
            </equal>
            <not>
             <equal>
              <clip pos="1" side="sl" part="a_ref"/>
              <lit-tag v="ref"/>
              </equal>    
            </not>
            </and>
          </test>
        <let>
            <var n="new_nbr"/><lit-tag v="sp"/>
        </let>
       </when>
       <otherwise>
        <let>
            <var n="new_nbr"/><clip pos="1" side="sl" part="a_nbr"/>
        </let>
       </otherwise>
      </choose>
  </def-macro>

  <def-macro n="pnbr-pers" npar="1">
      <choose>
       <when>
         <test>
           <equal>
              <clip pos="1" side="sl" part="a_pers"/>
              <lit-tag v="p2"/>
            </equal>
          </test>
        <let>
            <var n="new_nbr"/><lit-tag v="psp"/>
        </let>
       </when>
       <otherwise>
        <let>
            <var n="new_nbr"/><clip pos="1" side="sl" part="a_pnbr"/>
        </let>
       </otherwise>
      </choose>
  </def-macro> 
 
  <def-macro n="pers-identifier" npar="1">
    <let><var n="pers_nbr"/><clip pos="1" side="sl" part="a_pers"/></let>
    <append n="pers_nbr"> <clip pos="1" side="sl" part="a_nbr"/> </append>
  </def-macro> 

  <def-macro n="am-is-are" npar="1">
    <choose>
      <when> 
        <test>
          <or>
            <and>
              <equal>
                <clip pos="1" side="tl" part="a_pers"/>
                <lit-tag v="p1"/>
              </equal>
              <equal>
                <clip pos="1" side="tl" part="a_nbr"/>
                <lit-tag v="sg"/>
              </equal>
            </and>
            <and>
              <equal>
                <clip pos="1" side="tl" part="a_pers"/>
                <lit-tag v="p3"/>
              </equal>
              <equal>
                <clip pos="1" side="tl" part="a_nbr"/>                
				        <lit-tag v="sg"/>
              </equal>
            </and>
          </or>
        </test>
          <let> <var n="being"/> <concat><clip pos="1" side="tl" part="a_pers"/><clip pos="1" side="tl" part="a_nbr"/></concat> </let>
      </when>
    </choose>
  </def-macro>

  <def-macro n="third_person" npar="1">
    <choose>
      <when> 
        <test>
          <and>
            <equal>
              <clip pos="1" side="tl" part="a_tense"/>
              <lit-tag v="pres"/>
            </equal>
            <equal>
              <clip pos="1" side="tl" part="a_pers"/>
              <lit-tag v="p3"/>
            </equal>
            <equal>
              <clip pos="1" side="tl" part="a_nbr"/>
              <lit-tag v="sg"/>
            </equal>
          </and>
        </test>
          <let> <var n="third_p_sg"/> <concat><clip pos="1" side="tl" part="a_pers"/><clip pos="1" side="tl" part="a_nbr"/></concat> </let>
      </when>
    </choose>
  </def-macro>
  
  <def-macro n="was-were" npar="1">
    <choose>
      <when> 
        <test>
          <or>
          <and>
            <equal>
              <clip pos="1" side="tl" part="a_vbser"/>
              <lit-tag v="vbser"/>
            </equal>
            <equal>
              <clip pos="1" side="tl" part="a_tense"/>
              <lit-tag v="past"/>
            </equal>
            <equal>
              <clip pos="1" side="tl" part="a_tense"/>
              <lit-tag v="past"/>
            </equal>
            <equal>
              <clip pos="1" side="tl" part="a_pers"/>
              <lit-tag v="p3"/>
            </equal>
            <equal>
              <clip pos="1" side="tl" part="a_nbr"/>
              <lit-tag v="sg"/>
            </equal>
          </and>
          <and>
            <equal>
              <clip pos="1" side="tl" part="a_vbser"/>
              <lit-tag v="vbser"/>
            </equal>          
          <equal>
              <clip pos="1" side="tl" part="a_tense"/>
              <lit-tag v="past"/>
          </equal>
          <equal>
              <clip pos="1" side="tl" part="a_pers"/>
              <lit-tag v="p1"/>
           </equal>
           <equal>
              <clip pos="1" side="tl" part="a_nbr"/>
              <lit-tag v="sg"/>
          </equal>
          </and>
        </or>
       </test>
        <let> <var n="was_were"/> <concat><clip pos="1" side="tl" part="a_pers"/><clip pos="1" side="tl" part="a_nbr"/></concat> </let>
      </when>
    </choose>
  </def-macro>

   <def-macro n="do-does-did" npar="1">
    <choose>
      <when> 
        <test>
          <or>
            <and>
             <equal>
                <clip pos="1" side="tl" part="a_tense"/>
                <lit-tag v="pres"/>
             </equal>
             <equal>
                <clip pos="1" side="tl" part="a_pers"/>
                <lit-tag v="p3"/>
             </equal>
             <equal>
                <clip pos="1" side="tl" part="a_nbr"/>
                <lit-tag v="sg"/>
             </equal>
            </and>
            <and>
            <equal>
              <clip pos="1" side="tl" part="a_tense"/>
              <lit-tag v="pres"/>
            </equal>
		<equal>
              <clip pos="1" side="tl" part="a_tense"/>
              <lit-tag v="past"/>
            </equal>
    </and>
    </or>
    </test>
        <let> <var n="ddd"/> <concat><lit-tag v="p3"/><lit-tag v="sg"/></concat> 
        </let>
      </when>
    </choose>
  </def-macro>

   <def-macro n="check-not-proper" npar="1">
    <choose>
      <when> 
        <test>
          <and>
            <not>
              <equal>
                <clip pos="1" side="tl" part="a_unk"/>
                <lit-tag v="unk"/>
              </equal>
            </not>
            <equal>
              <clip pos="1" side="tl" part="a_ind"/>
              <lit-tag v="ind"/>
            </equal>
          </and>
        </test>
        <let> <var n="the_var"/> 
              <concat> <lit v="a"/>
                       <lit-tag v="det"/>
                       <lit-tag v="ind"/>
                       <lit-tag v="sg"/>
              </concat> </let>
      </when>
      <when> 
        <test>
          <and>
            <not>
              <equal>
                <clip pos="1" side="tl" part="a_nom"/>
                <lit-tag v="n"/>
              </equal>
            </not>
            <not>
              <equal>
                <clip pos="1" side="tl" part="a_ind"/>
                <lit-tag v="ind"/>
              </equal>
            </not>
           </and> 
        </test>
        <let> <var n="the_var"/> 
              <concat> <lit v="the"/>
                       <lit-tag v="det"/>
                       <lit-tag v="def"/>
                       <lit-tag v="sp"/>
              </concat> </let>
      </when>

    </choose>
  </def-macro>
  
  <def-macro n="check-neg-simple-verb" npar="1">
    <choose>
      <when> 
        <test>
            <equal>
                <clip pos="1" side="tl" part="a_neg"/>
                <lit-tag v="neg"/>
              </equal>
        </test>
              <call-macro n="third_person"><with-param pos="1"/></call-macro>
              <let> <var n="neg_var"/> 
              <concat> <lu>
                         <lit v="do"/>
                         <lit-tag v="vbdo"/>
                         <clip pos="1" side="tl" part="a_tense"/>
                         <var n="third_p_sg"/>
                       </lu>
                       <lu>
                         <lit v="not"/>
                         <lit-tag v="adv"/>
                       </lu>
                       <b />
              </concat> </let>
              <let>
                <clip pos="1" side="tl" part="a_tense"/>
                <lit-tag v="pres"/>
              </let>
              <let>
                 <clip pos="1" side="tl" part="a_pers"/>
                 <lit-tag v="p1"/>
              </let>
      </when>
    </choose>
  </def-macro>
  
  <def-macro n="check-not" npar="1">
    <choose>
      <when> 
        <test>
            <equal>
                <clip pos="1" side="tl" part="a_neg"/>
                <lit-tag v="neg"/>
              </equal>
        </test>
              <let> <var n="neg_var"/> 
              <concat> 
                        <b />
                        <lu>
                         <lit v="not"/>
                         <lit-tag v="adv"/>
                       </lu>
              </concat> </let>
      </when>
    </choose>
  </def-macro>
  
  <def-macro n="check-sint-proper" npar="1">
    <choose>
      <when> 
        <test>
          <and>
            <not>
              <equal>
                <clip pos="1" side="tl" part="a_adj"/>
                <lit-tag v="adj"/>
              </equal>
            </not>
            <equal>
              <clip pos="1" side="tl" part="a_comp"/>
			  <lit-tag v="comp"/>
            </equal>
          </and>
        </test>
        <let> <var n="more_var"/> 
              <concat> <lit v="more"/>
                       <lit-tag v="det"/>
                       <lit-tag v="qnt"/>
                       <lit-tag v="sp"/>
              </concat> </let>
      </when>
      <when> 
        <test>
          <and>
            <not>
              <equal>
                <clip pos="1" side="tl" part="a_adj"/>
                <lit-tag v="adj"/>
              </equal>
            </not>
            <not>
              <equal>
                <clip pos="1" side="tl" part="a_sup"/>
				<lit-tag v="sup"/>
              </equal>
            </not>
           </and> 
        </test>
        <let> <var n="the_most_var"/> 
              <concat> <lit v="the most"/>
                       <lit-tag v="det"/>
                       <lit-tag v="qnt"/>
                       <lit-tag v="sp"/>
              </concat> </let>
      </when>

  </choose>
  </def-macro>
</section-def-macros>

<section-rules> 
  	<rule comment="1.noun-pos, adj, verb">
  	   <pattern>
  	     <pattern-item n="pos"/>
  	     <pattern-item n="adjective"/>
  	     <pattern-item n="vrb"/>
