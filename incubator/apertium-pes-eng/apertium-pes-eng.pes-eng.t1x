  <rule comment="148.vaux + simple verb"> 
  <pattern> 
    <pattern-item n="c_vaux"/>  
   
   </pattern>
  <action>
    <call-macro n="nbr-pers"><with-param pos="1"/></call-macro>
    <call-macro n="check-not"><with-param pos="1"/></call-macro>
    <out>
      <chunk name="vaux">
        <tags>
           <tag><lit-tag v="SVAUXV"/></tag>
        <tag><clip pos="1" side="tl" part="a_pers"/></tag>
      </tags>
      <lu>
       <clip pos="1" side="tl" part="lem"/>
        <lit-tag v="vaux"/>
        <lit-tag v="inf"/>
      </lu>
      <var n="neg_var"/>   
    </chunk>
    </out>
  </action>
</rule>
  
