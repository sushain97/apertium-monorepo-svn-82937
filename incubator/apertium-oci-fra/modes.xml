      </program>
      <program name="lt-proc $1">
        <file name="fra-oci.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="fra-oci.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

