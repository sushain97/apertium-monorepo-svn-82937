          <clip pos="1" side="tl" part="nombre"/>
          <clip pos="4" side="tl" part="lemq"/>
        </lu>
      </out>

    </def-macro>


    <def-macro n="conj_verbe_neg_prn-obj" npar="4">
    <!--  conjugue un verbe avec ne ... pas/plus -->
    <!--  param1 : nom ou pronom, param2 : Ne/ne plu,
          param3 : verbe, param4 pronom objet -->

      <out>
