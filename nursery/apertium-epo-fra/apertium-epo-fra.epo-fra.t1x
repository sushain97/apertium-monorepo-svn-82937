    <def-macro n="conj_etre_adj_neg" npar="4">
    <!--  conjugue le verbe etre suivi d'un adjectif -->
    <!--  param1 : nom ou pronom, param2 : Ne/ne plu,
          param3 : verbe, param4 : adjectif -->

      <call-macro n="conj_verbe_neg">
        <with-param pos="1"/>
        <with-param pos="2"/>
        <with-param pos="3"/>
