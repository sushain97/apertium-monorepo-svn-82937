          <clip pos="1" side="tl" part="genre"/>
          <clip pos="1" side="tl" part="nombre"/>
          <clip pos="4" side="tl" part="lemq"/>
        </lu>
      </out>

    </def-macro>


    <def-macro n="conj_etre_verbe_pp_neg" npar="4">
    <!--  conjugue le verbe etre suivi d'un verbe au participe passé -->
    <!--  param1 : nom ou pronom, param2 : Ne/ne plu,
          param3 : verbe, param4 : participe passé -->

      <call-macro n="conj_verbe_neg">
        <with-param pos="1"/>
        <with-param pos="2"/>
        <with-param pos="3"/>
