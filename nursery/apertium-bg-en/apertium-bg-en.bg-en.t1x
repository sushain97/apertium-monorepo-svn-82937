  <section-rules>


    <rule comment="REGLA: prn_ref_clt">
      <pattern>
        <pattern-item n="prn_ref_clt"/>
      </pattern>
      <action>
        <out>
        <b/>
        </out>
      </action>
    </rule>


    <!-- Sintagmas nominales -->

    <rule comment="REGLA: prn_ref_pos">
      <pattern>
        <pattern-item n="prn_ref_pos"/>
      </pattern>
      <action>
        <let>
          <clip pos="1" side="tl" part="lemh"/> 
          <lit v="its"/>
        </let>
        <choose>
          <when> 
            <test>
              <and>
                <equal>
                  <var n="cur_v_person"/>
                  <lit-tag v="p1"/>
                </equal>
                <equal>
                  <var n="cur_v_number"/>
