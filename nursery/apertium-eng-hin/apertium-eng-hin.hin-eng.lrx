  <rule>
    <match lemma="उत्पाद" tags="n.*"><select lemma="product" tags="n.*"/></match> 
  </rule>
  <rule>
    <match lemma="स्वतंत्र" tags="adj.*"><select lemma="independent" tags="adj.*"/></match> 
  </rule>
  <rule>
    <match lemma="शताब्दी" tags="adj.*"><select lemma="century" tags="adj.*"/></match> 
  </rule>
  <rule>
    <match lemma="रोज़ाना" tags="adj.*"><select lemma="everyday" tags="adj.*"/></match> 
  </rule>
  <rule>
    <match lemma="कुल" tags="adj.*"><select lemma="total" tags="adj.*"/></match> 
  </rule>
  <rule>
