  <mode name="es-cs" install="yes">
    <pipeline>
      <program name="lt-proc">
        <file name="es-cs.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="es-cs.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
