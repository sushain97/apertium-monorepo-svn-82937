      </program>
      <program name="apertium-postchunk">
        <file name="apertium-es-cs.es-cs.t3x"/>
        <file name="es-cs.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="es-cs.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="es-cs.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

