      <program name="apertium-postchunk">
        <file name="apertium-ro-it.ro-it.t3x"/>
        <file name="ro-it.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="ro-it.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="ro-it.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

