<?xml version="1.0" encoding="UTF-8"?>
<modes>
  <mode name="es-ssp" install="yes">
    <pipeline>
      <program name="lt-proc">
        <file name="es-ssp.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g">
        <file name="es-ssp.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-es-ssp.es-ssp.t1x"/>
        <file name="es-ssp.t1x.bin"/>
        <file name="es-ssp.autobil.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-es-ssp.es-ssp.t2x"/>
        <file name="es-ssp.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-es-ssp.es-ssp.t3x"/>
        <file name="es-ssp.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="es-ssp.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="es-ssp.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

  <mode name="es-ssp-anmor" install="no">
