          </let>
    </def-macro>

    <def-macro n="agreement-attr-2Param" npar="2"> <!-- pl prn, num, ord , full agreement, exc com -->
       <choose>
         <when>        <!-- with pl.com -->
           <test><or>
            <and>
              <equal><clip pos="2" side="tl" part="a_case"/><lit-tag v="com"/></equal>
              <equal><clip pos="2" side="tl" part="a_nbr"/><lit-tag v="pl"/></equal>
            </and>
            <and>
              <equal><clip pos="2" side="tl" part="a_case"/><lit-tag v="loc"/></equal>
              <equal><clip pos="2" side="tl" part="a_nbr"/><lit-tag v="sg"/></equal>
            </and>
            <and>
              <equal><clip pos="2" side="tl" part="a_case"/><lit-tag v="ill"/></equal>
              <equal><clip pos="2" side="tl" part="a_nbr"/><lit-tag v="sg"/></equal>
            </and>
         </or></test>
        <let>
          <clip pos="1" side="tl" part="tags"/>
           <concat>
             <clip pos="1" side="tl" part="a_npagree_type"/>
             <clip pos="1" side="tl" part="a_comp-sup"/>
             <clip pos="2" side="tl" part="a_nbr"/>
             <lit-tag v="gen"/>
             <clip pos="1" side="tl" part="a_foc"/>
           </concat>
        </let>
        </when>
        <otherwise>
                <!-- default -->
           <let>
             <clip pos="1" side="tl" part="tags"/>
             <concat>
               <clip pos="1" side="tl" part="a_npagree_type"/>
               <clip pos="1" side="tl" part="a_comp-sup"/>
               <clip pos="2" side="tl" part="a_nbr"/>
               <clip pos="2" side="tl" part="a_case"/>
             </concat>
           </let>
        </otherwise>
      </choose>
    </def-macro>





