  <section-def-macros>
    <def-macro n="f_fix_trans1" npar="1">
      <choose>
        <when>
          <test>
            <and>
              <in caseless="yes"><clip pos="1" side="tl" part="lem"/><list n="v_iv_with_pass"/></in>
              <equal><clip pos="1" side="sl" part="pass"/><lit-tag v="pass"/></equal>
            </and>
          </test>
          <let><clip pos="1" side="tl" part="a_trans"/><lit-tag v="tv"/></let> 
        </when>
      </choose>
      <choose>
        <when>
          <test>
            <and>
              <not><in caseless="yes"><clip pos="1" side="tl" part="lem"/><list n="v_iv_with_pass"/></in></not>
              <equal><clip pos="1" side="tl" part="a_trans"/><lit-tag v="iv"/></equal>
              <equal><clip pos="1" side="sl" part="pass"/><lit-tag v="pass"/></equal>
            </and>
          </test>
          <let><clip pos="1" side="tl" part="pass"/><lit v=""/></let> 
        </when>
      </choose>

      <choose>
        <when>
          <test>
              <equal><clip pos="1" side="tl" part="abil"/><lit-tag v="abil"/></equal>
          </test>
          <let><clip pos="1" side="tl" part="abil"/><lit v=""/></let> 
        </when>
      </choose>
    </def-macro>

    <def-macro n="f_conv_case1" npar="1">
      <let><var n="c_post"/><lit v=""/></let>
      <choose>
        <when>
          <test>
            <and>
              <equal><clip pos="1" side="sl" part="case"/><lit v=""/></equal>
              <not><equal><clip pos="1" side="tl" part="case"/><lit-tag v="locattr"/></equal></not>
            </and>
          </test>
          <let><var n="c_case"/><lit-tag v="nom"/></let>
        </when>
        <when>
          <test>
            <equal><clip pos="1" side="tl" part="case"/><lit-tag v="locattr"/></equal>
          </test>
          <let><var n="c_case"/><lit-tag v="locattr"/></let>
        </when>
        <when>
          <test>
            <equal><clip pos="1" side="sl" part="case"/><lit-tag v="loc.ki"/></equal>
          </test>
          <let><var n="c_case"/><lit-tag v="locattr"/></let>
        </when>
        <when>
          <test>
            <equal><clip pos="1" side="sl" part="case"/><lit-tag v="ins"/></equal>
          </test>
          <let><var n="c_case"/><lit-tag v="nom"/></let>
          <let><var n="c_post"/><concat><lit v=" ^менен"/><lit-tag v="post"/><lit v="$"/></concat></let>
        </when>
        <otherwise>
          <let><var n="c_case"/><clip pos="1" side="sl" part="case"/></let>
        </otherwise>
      </choose>
    </def-macro>  

    <def-macro n="f_conv_tens1" npar="1">
      <choose>
        <when>
          <test>
            <equal><clip pos="1" side="tl" part="a_tense"/><lit-tag v="t_narr"/></equal>
          </test>
          <let><clip pos="1" side="tl" part="a_tense"/><lit-tag v="past"/></let>
          <let><var n="c_tens"/><lit-tag v="past"/></let>
        </when>
        <when>
          <test>
            <equal><clip pos="1" side="tl" part="a_tense"/><lit-tag v="past"/></equal>
          </test>
          <let><clip pos="1" side="tl" part="a_tense"/><lit-tag v="ifi"/></let>
          <let><var n="c_tens"/><lit-tag v="ifi"/></let>
        </when>
        <when>
          <test>
            <equal><clip pos="1" side="tl" part="a_tense"/><lit-tag v="t_opt"/></equal>
          </test>
          <let><clip pos="1" side="tl" part="a_tense"/><lit-tag v="opt"/></let>
          <let><var n="c_tens"/><lit-tag v="opt"/></let>
        </when>
        <when>
          <test>
            <equal><clip pos="1" side="tl" part="a_tense"/><lit-tag v="t_fut"/></equal>
          </test>
          <let><clip pos="1" side="tl" part="a_tense"/><lit-tag v="fut"/></let>
          <let><var n="c_tens"/><lit-tag v="fut"/></let>
        </when>
        <when>
          <test>
