  <section-def-macros>
    <def-macro n="nonfin1" npar="1">
      <choose>
        <when>
          <test><equal><clip pos="1" side="sl" part="v_nonfin"/><lit-tag v="inf"/></equal></test>
          <let>
            <clip pos="1" side="tl" part="v_nonfin"/>
            <concat>
            <lit-tag v="ger_pres"/>
            <lit-tag v="nom"/>
            </concat>
          </let>
        </when>
      <when>
          <test><equal><clip pos="1" side="sl" part="v_nonfin"/><lit-tag v="gna_when"/></equal></test>
          <let>
            <clip pos="1" side="tl" part="v_nonfin"/>
            <concat>
            <lit-tag v="ger_past"/>
            <lit-tag v="loc"/>
            </concat>
          </let>
        </when>
        <when>
          <test><equal><clip pos="1" side="sl" part="v_nonfin"/><lit-tag v="cond"/></equal></test>
          <let>
            <clip pos="1" side="tl" part="v_nonfin"/>
            <lit-tag v="gna_cond"/>
          </let>
        </when>
      </choose>
    </def-macro>
  </section-def-macros>


  <section-rules>

    <rule comment="ger_when"> 
      <pattern>
        <pattern-item n="ger_when"/>
      </pattern>
      <action>
            <let><clip pos="1" side="tl" part="v_nonfin"/><lit-tag v="ger_past"/></let>
            <out>
              <lu>
                <clip pos="1" side="tl" part="whole"/>
              </lu>
            </out>
      </action>
    </rule>

    <rule comment="abil-ger_fut-nom kadar">
      <pattern>
        <pattern-item n="abil-ger_fut-nom"/>
        <pattern-item n="kadar"/>
      </pattern>
      <action>
            <let><clip pos="1" side="tl" part="case"/><lit-tag v="dat"/></let>
            <let><clip pos="1" side="tl" part="v_nonfin"/><lit-tag v="ger_past"/></let>
            <out>
              <lu>
                <clip pos="1" side="tl" part="lemh"/>
                <clip pos="1" side="tl" part="a_verb"/>
                <clip pos="1" side="tl" part="trans"/>
                <clip pos="1" side="tl" part="deriv"/>
                <clip pos="1" side="tl" part="a_neg"/>
                <lit-tag v="prc_impf"/>
                <clip pos="1" side="tl" part="lemq"/>
              </lu>
              <b pos="1"/>
              <lu>
                <lit v="ал"/>
                <lit-tag v="vaux"/>
                <clip pos="1" side="tl" part="v_nonfin"/>
                <clip pos="1" side="tl" part="num"/>
                <clip pos="1" side="tl" part="case"/>
              </lu>
              <b/>
              <lu>
                <clip pos="2" side="tl" part="whole"/>
              </lu>
            </out>
      </action>
    </rule>


    <rule comment="ger_fut-nom kadar">
      <pattern>
        <pattern-item n="ger_fut-nom"/>
        <pattern-item n="kadar"/>
      </pattern>
      <action>
            <let><clip pos="1" side="tl" part="case"/><lit-tag v="dat"/></let>
            <let><clip pos="1" side="tl" part="v_nonfin"/><lit-tag v="ger_past"/></let>
            <out>
              <lu>
                <clip pos="1" side="tl" part="whole"/>
              </lu>
              <b pos="1"/>
              <lu>
                <clip pos="2" side="tl" part="whole"/>
              </lu>
            </out>
      </action>
    </rule>

    <rule comment="iste-ger_past-px kadar">
      <pattern>
        <pattern-item n="iste-ger_past-px"/>
        <pattern-item n="kadar"/>
      </pattern>
      <action>
            <out>
              <lu>
                <get-case-from pos="1">
                  <lit v="каалаганча"/>
                </get-case-from>
                <lit-tag v="det.qnt"/>
              </lu>
            </out>
      </action>


    </rule>
  
    <rule comment="n-acc iç-px-dat çek-aor cop-ifi"> 
      <pattern>
        <pattern-item n="n-acc"/>
        <pattern-item n="iç-px-dat"/>
        <pattern-item n="çek-aor"/>
        <pattern-item n="cop-ifi"/>
      </pattern>
      <action>
        <let><clip pos="1" side="tl" part="case"/><lit-tag v="dat"/></let>
        <out>
          <lu>
            <clip pos="1" side="tl" part="whole"/>
          </lu>
          <b pos="1"/>
          <lu>
            <get-case-from pos="2">
              <lit v="суктан"/>
            </get-case-from>
            <lit-tag v="v.iv"/>
            <clip pos="4" side="tl" part="neg"/>
            <lit-tag v="pcond"/>
            <clip pos="4" side="tl" part="person"/>
            <clip pos="4" side="tl" part="num"/>
          </lu>
        </out>
      </action>
    </rule>

    <rule comment="num-abstr n"> 
      <pattern>
        <pattern-item n="num-abstr"/>
        <pattern-item n="n"/>
      </pattern>
      <action>
        <let><clip pos="1" side="tl" part="a_abstr"/><lit v=""/></let>
            <out>
              <lu>
                <clip pos="1" side="tl" part="whole"/>
              </lu>
              <b/>
              <lu>
                <lit v="жаш"/>
                <lit-tag v="n.loc.attr"/>
              </lu>
              <b pos="1"/>
              <lu>
                <clip pos="2" side="tl" part="whole"/>
              </lu>
            </out>
      </action>
    </rule>

    <rule comment="bir n-nom n-px3sp-nom"> 
      <pattern>
        <pattern-item n="bir"/>
        <pattern-item n="n-nom"/>
        <pattern-item n="n-px3sp-nom"/>
      </pattern>
      <action>
        <let><clip pos="2" side="tl" part="case"/><lit-tag v="gen"/></let> 
        <choose>
          <when>
            <test><equal><clip pos="1" side="tl" part="case"/><lit-tag v="ins"/></equal></test>
            <let><clip pos="1" side="tl" part="case"/><lit-tag v="nom"/></let> 
            <out>
              <lu>
                <clip pos="1" side="tl" part="whole"/>
              </lu>
              <b pos="1"/>
              <lu>
                <clip pos="2" side="tl" part="whole"/>
              </lu>
              <b pos="2"/>
              <lu>
                <clip pos="3" side="tl" part="whole"/>
              </lu>
              <b/>
              <lu>
                <lit v="менен"/> 
                <lit-tag v="post"/>
              </lu>
            </out>
          </when>
          <otherwise>
            <out>
              <lu>
                <clip pos="1" side="tl" part="whole"/>
              </lu>
              <b pos="1"/>
              <lu>
                <clip pos="2" side="tl" part="whole"/>
              </lu>
              <b pos="2"/>
              <lu>
                <clip pos="3" side="tl" part="whole"/>
              </lu>
            </out>
          </otherwise>
        </choose>
      </action>
    </rule>

    <rule comment="adj qst cop"> 
