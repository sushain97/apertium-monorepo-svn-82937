<rule comment="fa els canvis de prepo">
  <pattern>
    <pattern-item n="prep"/>
  </pattern>
  <action>
    <choose>
      <when>
        <test>
          <equal>
            <clip pos="0" part="tprep"/>
            <lit-tag v="a"/>
          </equal>
        </test>
        <let>
           <clip pos="1" part="lem"/>
           <lit v="a"/>
        </let>
     </when>
     <when>
       <test>
         <equal>
           <clip pos="0" part="tprep"/>
           <lit-tag v="en"/>
         </equal> 
       </test>
       <let>
         <clip pos="1" part="lem"/>
         <lit v="en"/>
       </let>
     </when>
     <when>
       <test>
         <equal>
           <clip pos="0" part="tprep"/>
           <lit-tag v="para"/>
         </equal>
       </test>
       <let>
         <clip pos="1" part="lem"/>
         <lit v="para"/>
       </let>
     </when>
    </choose>
    <out>
     <lu>
       <clip pos="1" part="lem"/>
       <clip pos="1" part="a_pr"/>
     </lu> 
    </out>     
</action>
</rule> 


    
