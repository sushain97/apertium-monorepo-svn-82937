<rule comment="NEW RULE: QAdvP NP VP">
    <!-- when he came ол келгенде-->
     <pattern>
       <pattern-item n="QAdvP"/>
       <pattern-item n="NP"/>
       <pattern-item n="VP"/>
         </pattern>
     <action>
<call-macro n="change-case-upper">  
<with-param pos="1"/>
  </call-macro> 
<call-macro n="change-case-lower">  
<with-param pos="2"/>
  </call-macro> 
<call-macro n="choose-number-person">  
<with-param pos="2"/>
  </call-macro>

           <out>
           <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk> 
      <b pos="1"/>
      <b pos="2"/>
       <chunk>
            <clip pos="3" part="lem"/>
            <lit-tag v="VP"/>
