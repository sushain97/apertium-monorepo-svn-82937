<def-macro n="build-case-ant" npar="1">
<choose> 
<when>
   <test>
     
          <equal><clip pos="1"  part="a_np"/><lit-tag  v="np.ant.f"/></equal>
     
    </test>      
          <let><var  n="case"/><lit-tag v="dat"/></let>
</when>
  <otherwise>
          <let>
            <var n="case"/><lit-tag v="acc"/>
          </let>
        </otherwise>
</choose>

</def-macro>


<def-macro n="choose-number-person" npar="1">  <!--NP VP agreement-->
<choose> 
<when>
   <test>
      <and>
          <equal><clip pos="1"  part="a_pers"/><lit  v=""/></equal>
          <equal><clip pos="1"  part="a_nbr"/><lit v=""/></equal>
       </and>
    </test>      
          <let><var  n="person"/><lit-tag v="p3"/></let>
          <let><var  n="number"/><lit-tag v="sg"/></let>
</when>
<otherwise>
   <let><var  n="person"/><clip pos="1"  part="a_pers"/></let>
          <let><var  n="number"/><clip pos="1"  part="a_nbr"/></let>

      </otherwise>
</choose> 
</def-macro>


<def-macro n="change-case-upper" npar="1">  <!--change case to upper for first word-->

       <modify-case>
        <clip pos="1" part="lem"/>
          <lit v="Aa"/>
        </modify-case>
             <let>
        <clip pos="1"  part="lem"/>
		<lit v="Aa"/>
	      </let>
      
</def-macro>

<def-macro n="change-case-lower" npar="1">  <!--change case to lower for other word-->

       <modify-case>
        <clip pos="1" part="lem"/>
          <lit v="Aa"/>
        </modify-case>
             <let>
        <clip pos="1"  part="lem"/>
		<lit v="aa"/>
	      </let>
      
</def-macro>
<def-macro n="build-possessive" npar="1">
<choose> 
<when>
   <test>
      <and>
          <equal><clip pos="1"  part="a_pers"/><lit-tag  v="p1"/></equal>
          <equal><clip pos="1"  part="a_nbr"/><lit-tag v="sg"/></equal>
       </and>
    </test>      
          <let><var  n="possessive"/><lit-tag v="px1sg"/></let>
</when>


<when>
   <test>
      <and>
          <equal><clip  pos="1"  part="a_pers"/><lit-tag  v="p1"/></equal>
          <equal><clip pos="1"  part="a_nbr"/><lit-tag v="pl"/></equal>
       </and>
    </test>      
          <let><var  n="possessive"/><lit-tag v="px1pl"/></let>
</when>
<when>
   <test>
      <and>
          <equal><clip pos="1"  part="a_pers"/><lit-tag  v="p2"/></equal>
          <equal><clip pos="1"  part="a_nbr"/><lit-tag v="sg"/></equal>
	
       </and>
    </test>      
<let><var  n="possessive"/><lit-tag v="px2sg"/></let>

</when>
<when>
   <test>
      <and>
          <equal><clip pos="1"  part="a_pers"/><lit-tag  v="p2"/></equal>
          <equal><clip pos="1"  part="a_nbr"/><lit-tag v="sg"/></equal>
	
       </and>
    </test>      
<let><var  n="possessive"/><lit-tag v="px2sg"/></let>

</when>
<when>
   <test>
          <equal><clip  pos="1"  part="a_pers"/><lit-tag  v="p3"/></equal>
    </test>      
          <let><var  n="possessive"/><lit-tag v="px3sp"/></let>
</when>
<otherwise>
          <let><var  n="possessive"/><lit-tag v="PXD"/></let>
</otherwise>

</choose>

</def-macro>



