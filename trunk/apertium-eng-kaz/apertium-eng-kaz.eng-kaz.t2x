<rule comment="NEW RULE PP GenP → GenP PP">
<!-- reorders PP and GenP and will set possessive on PP -->
  <pattern> 
     <pattern-item n="PP"/>
     <pattern-item n="GenP"/>
  </pattern>
     <action>
     <call-macro n="build-possessive">
        <with-param pos="2"/>
     </call-macro>
      <out>
          <b pos="1"/>
          <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>
          <b/>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="a_phrase"/>
            <clip pos="1" part="a_nbr"/>
            <clip pos="1" part="a_pers"/>
            <var n="possessive"/>
            <clip pos="1" part="a_cas"/>
            <clip pos="1" part="chcontent"/>
          </chunk>  
      </out>
     </action>
</rule>

<rule comment="NEW RULE PP GenP GenP → GenP GenP PP">
<!-- only 1 NIng -->
